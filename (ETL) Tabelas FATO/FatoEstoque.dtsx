    <DTS:Executable
      DTS:refId="Package\Estoque Consolidado por PN"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Tarefa Executar SQL"
      DTS:Disabled="True"
      DTS:DTSID="{011C92D6-8167-4AE0-9AAE-5DE3552DC63C}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Estoque Consolidado por PN"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{212658AD-9DF7-420C-A423-6A8373948A16}"
          SQLTask:SqlStatementSource="&#xA;Drop table EstoqueConsolidado&#xA;go&#xA;Declare @DataMaxima Int &#xA;Declare @DataPeriodo varchar(6) &#xA;Select @DataMaxima = (Select Max(T.IDSKTTempMes) From St.EstoqueFilial As M Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T On (M.PeriodoMov = T.DataPeriodo))&#xA;Select @DataPeriodo =(Select Max(PeriodoMov) From St.EstoqueFilial)&#xA;&#xA;Declare @Sql nVarchar(max)&#xA;Declare @Periodo1 varchar(10), @Periodo2 varchar(10), @Periodo3 varchar(10), @Periodo4 varchar(10), @Periodo5 varchar(10), @Periodo6 varchar(10), @Periodo7 varchar(10), @Periodo8 varchar(10), @Periodo9 varchar(10), @Periodo10 varchar(10), @Periodo11 varchar(10), @Periodo12 varchar(10)&#xA;Set @Periodo1 = (Select Distinct T.MesBarra01 From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B.Periodo = T.DataPeriodo) Where T.IDSKTTempMes = @DataMaxima - 11 )&#xA;Set @Periodo2 = (Select Distinct T.MesBarra01 From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B.Periodo = T.DataPeriodo) Where T.IDSKTTempMes = @DataMaxima - 10 )&#xA;Set @Periodo3 = (Select Distinct T.MesBarra01 From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B.Periodo = T.DataPeriodo) Where T.IDSKTTempMes = @DataMaxima - 9 )&#xA;Set @Periodo4 = (Select Distinct T.MesBarra01 From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B.Periodo = T.DataPeriodo) Where T.IDSKTTempMes = @DataMaxima - 8 )&#xA;Set @Periodo5 = (Select Distinct T.MesBarra01 From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B.Periodo = T.DataPeriodo) Where T.IDSKTTempMes = @DataMaxima - 7 )&#xA;Set @Periodo6 = (Select Distinct T.MesBarra01 From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B.Periodo = T.DataPeriodo) Where T.IDSKTTempMes = @DataMaxima - 6 )&#xA;Set @Periodo7 = (Select Distinct T.MesBarra01 From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B.Periodo = T.DataPeriodo) Where T.IDSKTTempMes = @DataMaxima - 5 )&#xA;Set @Periodo8 = (Select Distinct T.MesBarra01 From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B.Periodo = T.DataPeriodo) Where T.IDSKTTempMes = @DataMaxima - 4 )&#xA;Set @Periodo9 = (Select Distinct T.MesBarra01 From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B.Periodo = T.DataPeriodo) Where T.IDSKTTempMes = @DataMaxima - 3 )&#xA;Set @Periodo10 = (Select Distinct T.MesBarra01 From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B.Periodo = T.DataPeriodo) Where T.IDSKTTempMes = @DataMaxima - 2 )&#xA;Set @Periodo11 = (Select Distinct T.MesBarra01 From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B.Periodo = T.DataPeriodo) Where T.IDSKTTempMes = @DataMaxima - 1 )&#xA;Set @Periodo12 = (Select Distinct T.MesBarra01 From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B.Periodo = T.DataPeriodo) Where T.IDSKTTempMes = @DataMaxima)&#xA;&#xA;Set @Sql = '&#xA;Declare @MesAtual Int&#xA;Select @MesAtual = (Select IDSKTTempMes from GHT_DATAWAREHOUSE.DW.DimTempoMes Where UltDdMes = EOMONTH(Getdate()))&#xA;&#xA;Select &#xA;E.NomeFilial,&#xA;B.PartNumber,&#xA;(Select IsNUll(Sum(QuantEstoque),0) from Ght_stage.St.EstoqueFilial As C3 (NOLOCK) Where C3.PeriodoMov = (Select Max(periodomov) from Ght_Stage.St.EstoqueFilial) And C3.CodEmpresa = B.CodEmpresa And B.PartNumber = C3.ProdNumPeca) As Est_FILIAL,&#xA;(Select IsNUll(Sum(QuantEstoque),0) from Ght_stage.St.EstoqueFilial As C3 (NOLOCK) Where C3.PeriodoMov = (Select Max(periodomov) from Ght_Stage.St.EstoqueFilial) And C3.CodEmpresa = 15 And B.PartNumber = C3.ProdNumPeca) As Est_CD_SP,&#xA;(Select IsNUll(Sum(QuantEstoque),0) from Ght_stage.St.EstoqueFilial As C3 (NOLOCK) Where C3.PeriodoMov = (Select Max(periodomov) from Ght_Stage.St.EstoqueFilial) And C3.CodEmpresa = 18 And B.PartNumber = C3.ProdNumPeca) As Est_HF100,&#xA;(Select IsNUll(Sum(QuantEstoque),0) from Ght_stage.St.EstoqueFilial As C3 (NOLOCK) Where C3.PeriodoMov = (Select Max(periodomov) from Ght_Stage.St.EstoqueFilial) And C3.CodEmpresa = 17 And B.PartNumber = C3.ProdNumPeca) As Est_PARA,&#xA;(Select IsNUll(Sum(QuantEstoque),0) from Ght_stage.St.EstoqueFilial As C3 (NOLOCK) Where C3.PeriodoMov = (Select Max(periodomov) from Ght_Stage.St.EstoqueFilial) And C3.CodEmpresa = 16 And B.PartNumber = C3.ProdNumPeca) As Est_CONTAGEM,&#xA;(Isnull((Select Sum(B1.Qtde) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.MesBarra01 = '+''''+@periodo1 +''''+' And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber),0)) As '+''''+@periodo1 +''''+',&#xA;(Isnull((Select Sum(B1.Qtde) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.MesBarra01 = '+''''+@Periodo2 +''''+' And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber),0)) As '+''''+@Periodo2 +''''+',&#xA;(Isnull((Select Sum(B1.Qtde) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.MesBarra01 = '+''''+@periodo3 +''''+' And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber),0)) As '+''''+@Periodo3 +''''+',&#xA;(Isnull((Select Sum(B1.Qtde) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.MesBarra01 = '+''''+@periodo4 +''''+' And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber),0)) As '+''''+@Periodo4 +''''+',&#xA;(Isnull((Select Sum(B1.Qtde) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.MesBarra01 = '+''''+@periodo5 +''''+' And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber),0)) As '+''''+@Periodo5 +''''+',&#xA;(Isnull((Select Sum(B1.Qtde) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.MesBarra01 = '+''''+@periodo6 +''''+' And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber),0)) As '+''''+@Periodo6 +''''+',&#xA;(Isnull((Select Sum(B1.Qtde) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.MesBarra01 = '+''''+@periodo7 +''''+' And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber),0)) As '+''''+@Periodo7 +''''+',&#xA;(Isnull((Select Sum(B1.Qtde) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.MesBarra01 = '+''''+@periodo8 +''''+' And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber),0)) As '+''''+@Periodo8 +''''+',&#xA;(Isnull((Select Sum(B1.Qtde) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.MesBarra01 = '+''''+@periodo9 +''''+' And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber),0)) As '+''''+@Periodo9 +''''+',&#xA;(Isnull((Select Sum(B1.Qtde) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.MesBarra01 = '+''''+@periodo10 +''''+' And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber),0)) As '+''''+@Periodo10 +''''+',&#xA;(Isnull((Select Sum(B1.Qtde) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.MesBarra01 = '+''''+@periodo11 +''''+' And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber),0)) As '+''''+@Periodo11 +''''+',&#xA;(Isnull((Select Sum(B1.Qtde) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.MesBarra01 = '+''''+@periodo12 +''''+' And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber),0)) As '+''''+@Periodo12 +''''+',&#xA;(Select Count(Distinct B1.Periodo) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.IDSKTTempMes Between @MesAtual - 11 And @MesAtual And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber) As MesesVenda,&#xA;(Select Sum(B1.Qtde) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.IDSKTTempMes Between @MesAtual - 11 And @MesAtual And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber) As TotalVenda,&#xA;(Select AVG(B1.Qtde) From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B1 Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B1.Periodo = T.DataPeriodo) Where T.IDSKTTempMes Between @MesAtual - 11 And @MesAtual And B.CodEmpresa = B1.CodEmpresa And B.PartNumber = B1.PartNumber) As MediaVenda,&#xA;(Select Sum(ISNUll(V.Saldototal, 0)) From GHT_DataWarehouse.DW.DimEmTransitoPN As V Where B.CodEmpresa = V.CodEmpresa And B.PartNumber = V.PartNumber) As SaldoEmtransito,&#xA;(Select Sum(IsNUll(V.SaldoSP_Transito,0)) From GHT_DataWarehouse.DW.DimEmTransitoPN As V Where B.CodEmpresa = V.CodEmpresa And B.PartNumber = V.PartNumber) As SaldoSPEmtransito,&#xA;(Select Sum(IsNUll(V.SaldoPA_Transito,0)) From GHT_DataWarehouse.DW.DimEmTransitoPN As V Where B.CodEmpresa = V.CodEmpresa And B.PartNumber = V.PartNumber) As SaldoPAEmtransito,&#xA;(Select Sum(IsNUll(V.SaldoMG_Transito,0)) From GHT_DataWarehouse.DW.DimEmTransitoPN As V Where B.CodEmpresa = V.CodEmpresa And B.PartNumber = V.PartNumber) As SaldoMGEmtransito&#xA;Into EstoqueConsolidado&#xA;From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As B  &#xA;Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) On (B.Periodo = T.DataPeriodo) &#xA;Inner Join GHT_DATAWAREHOUSE.DW.DimCadEmpresa As E (NOLOCK) On (B.CodEmpresa = E.CodEmpresa)&#xA;Where T.IDSKTTempMes Between @MesAtual - 11 And @MesAtual&#xA;Group by B.PartNumber, E.NomeFilial , B.CodEmpresa&#xA;'&#xA;Exec(@sql)" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
