                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Select
	Cast(E.ProdNumPeca As Varchar(16)) As PartNumber,
	Cast(E.FornecedorNome As varchar(16)) As Fornecedor,
    (Select 'Mais de 9 Meses Sem Vendas') As MesesSemVendas,
	CONVERT(Char(6),GetDate(),112) As Periodo
From GHT_STAGE.ST.EstoqueFilial As E
Left Join GHT_DATAWAREHOUSE.DW.DimMargemGerencial As M ON (E.ProdNumPeca = M.Produto And E.FornecedorNome = M.Fornecedor And E.CodEmpresa = M.CodFilial And E.PeriodoMov = M.Periodo)
Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T On (E.PeriodoMov = T.DataPeriodo)
Where T.IDSKTTempMes &gt; (Select MAX(T1.IDSKTTempMes) From GHT_DATAWAREHOUSE.DW.DimTempoMes As T1 Inner Join GHT_STAGE.ST.EstoqueFilial As E1 ON (E1.PeriodoMov = T1.DataPeriodo)) -10 And E.ProdNumPeca is not null
Group by E.ProdNumPeca, E.FornecedorNome
Having SUM(M.FatuSemImposto) is Null</property>
