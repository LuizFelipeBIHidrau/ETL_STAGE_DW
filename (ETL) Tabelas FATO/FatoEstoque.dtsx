                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @DataMaxima Int 
Declare @DataPeriodo varchar(6) 
Select @DataMaxima = (Select Max(T.IDSKTTempMes) From St.EstoqueFilial As M Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes (NOLOCK) As T On (M.PeriodoMov = T.DataPeriodo))
Select @DataPeriodo = (Select Max(PeriodoMov) From St.EstoqueFilial (NOLOCK))

;WITH CTE As(


Select Distinct
G.CodFilial As CodEmpresa,
G.Produto As PartNumber,
G.Fornecedor,
G.Periodo As PeriodoMov,
0 As Est
from  Ght_DataWarehouse.Dw.DimMargemGerencial As G
Left Join GHT_STAGE.ST.EstoqueFilial As E On (G.Produto = E.ProdNumPeca And G.Fornecedor = E.FornecedorNome And G.CodFilial = E.CodEmpresa And G.Periodo = E.PeriodoMov)
where E.ProdNumPeca is null

Union

Select Distinct
A1.CodEmpresa, 
A1.ProdNumPeca AS PartNumber,
A1.FornecedorNome As Fornecedor, 
A1.PeriodoMov, 
SUM(A1.QuantEstoque) As Est
--E1.EstadoUF As UF
From Ght_Stage.St.EstoqueFilial A1(NOLOCK)
--Inner Join GHT_STAGE.ST.CadEmpresa (NOLOCK) As E1 On (A1.CodEmpresa = E1.CodEmpresa)
Group By A1.CodEmpresa, A1.FornecedorNome, A1.PeriodoMov , A1.ProdNumPeca
)

Select
 S.IDSKEmpresa
,P.IDSKFicha
,F.IDSKFornecedor
,L.IDSKLinha
,A.IDSKFamilia
,M.IDSKMarca
,B.IDSKClassProd
,U.IDSKGrupo
,Z.IDSKDePara
,H.IDSKFichaTransito
,T.IDSKTTempMes
,(NullIf(H.Saldo ,0)) As SaldoEmTransito
,(NullIf(E.Est,0)) As Ght_Est
,(Nullif(P.CustoSemImposto,0) * E.Est) As Ght_CustoEst
,(NullIf(Q.Qtd, 0)) As Ght_QtdReserva
,(NullIf(J.Qtd, 0)) As Ght_QtdPendente
,(NullIf(D.Qtde,0)) As Ght_QtdVenda
,(Nullif(G.CustoVenda,0)) As Ght_CustoVenda
,(NullIf(G.Fatura,0)) As Ght_Fatura
,(Nullif(G.Mkp,0) / nullif(G.CustoVenda,0)) As Ght_Markup
,(Nullif(D1.Meses, 0)) As Ght_Ult12MesesVenda
,(Nullif(D1.Venda, 0)) As Ght_Ult12QtdVenda 
,(Select Sum(W.ConsumoOEM) From GHT_DATAWAREHOUSE.DW.DimCestaConsolidado (NOLOCK) As W Where W.Região = 'Norte' And E.PartNumber= W.PartNumber And E.PeriodoMov = W.Periodo And E.CodEmpresa In (12, 14, 16, 17, 18)) As ConsumoOEM_Norte
,(Select Sum(W1.ConsumoOEM) From GHT_DATAWAREHOUSE.DW.DimCestaConsolidado (NOLOCK) As W1 Where W1.Região = 'Sul' And E.PartNumber = W1.PartNumber And E.PeriodoMov = W1.Periodo And E.CodEmpresa In (12, 14, 16, 17, 18)) As ConsumoOEM_Sul
,(NullIf(X1.QtdeVendida, 0)) As Vale_QtdPendente
,(NullIf(I.Qtde,0)) As Vale_QtdVenda
,(NullIf(X1.FaturadoVale, 0)) As Vale_ValorPendente
,(NullIf(X.FaturadoVale, 0)) As Vale_Faturamento
,(NullIf(X.CustovendaVale,0)) as Vale_CustoVenda
,(Nullif(D3.Meses, 0)) As Vale_Ult12MesesVenda
,(Nullif(D3.Venda, 0)) As Vale_Ult12QtdVenda
,S.EstadoUF As UF
,O.QtdOc
,O.TotalOc
,O.DtOcEntrega
,(Select COUNT(Distinct T1.DataPeriodo) From GHT_DATAWAREHOUSE.DW.DimTempoMes (NOLOCK) T1 Where T1.IDSKTTempMes Between @DataMaxima - 11 And @DataMaxima And Not Exists(Select Distinct PartNumber, PeriodoMov From CTE E1 Left Join GHT_DATAWAREHOUSE.DW.DimMargemGerencial (NOLOCK) As M1 On (E1.PartNumber = M1.Produto And E1.PeriodoMov = M1.Periodo) Where M1.Produto Is not null and E1.PeriodoMov = T1.DataPeriodo And E1.PartNumber = E.PartNumber)And E.PeriodoMov = @DataPeriodo) As SemVendasUlt12Meses
,(Select COUNT(Distinct T1.DataPeriodo) From GHT_DATAWAREHOUSE.DW.DimTempoMes (NOLOCK) T1 Where T1.IDSKTTempMes Between @DataMaxima - 2 And @DataMaxima And Not Exists(Select Distinct PartNumber, PeriodoMov From CTE E1 Left Join GHT_DATAWAREHOUSE.DW.DimMargemGerencial(NOLOCK) As M1 On (E1.PartNumber = M1.Produto And E1.PeriodoMov = M1.Periodo) Where M1.Produto Is not null and E1.PeriodoMov = T1.DataPeriodo And E1.PartNumber = E.PartNumber)And E.PeriodoMov = @DataPeriodo) As SemVendasUlt3Meses
,(Select COUNT(Distinct T1.DataPeriodo) From GHT_DATAWAREHOUSE.DW.DimTempoMes (NOLOCK) T1 Where T1.IDSKTTempMes Between @DataMaxima - 5 And @DataMaxima And Not Exists(Select Distinct PartNumber, PeriodoMov From CTE E1 Left Join GHT_DATAWAREHOUSE.DW.DimMargemGerencial(NOLOCK) As M1 On (E1.PartNumber = M1.Produto And E1.PeriodoMov = M1.Periodo) Where M1.Produto Is not null and E1.PeriodoMov = T1.DataPeriodo And E1.PartNumber = E.PartNumber)And E.PeriodoMov = @DataPeriodo) As SemVendasUlt6Meses
,(Select COUNT(Distinct T1.DataPeriodo) From GHT_DATAWAREHOUSE.DW.DimTempoMes (NOLOCK) T1 Where T1.IDSKTTempMes Between @DataMaxima - 8 And @DataMaxima And Not Exists(Select Distinct PartNumber, PeriodoMov From CTE E1 Left Join GHT_DATAWAREHOUSE.DW.DimMargemGerencial(NOLOCK) As M1 On (E1.PartNumber = M1.Produto And E1.PeriodoMov = M1.Periodo) Where M1.Produto Is not null and E1.PeriodoMov = T1.DataPeriodo And E1.PartNumber = E.PartNumber)And E.PeriodoMov = @DataPeriodo) As SemVendasUlt9Meses
,(Select EstadoPonta from GHT_DATAWAREHOUSE.DW.DimCadContrato As Y (NOLOCK) Where Y.TipoContrato = 'Contrato Vale' And Y.EstadoPonta = 'SP' And E.PartNumber = Y.PartNumber) As PontaSP
,(Select EstadoPonta from GHT_DATAWAREHOUSE.DW.DimCadContrato As Y (NOLOCK) Where Y.TipoContrato = 'Contrato Vale' And Y.EstadoPonta = 'PA' And E.PartNumber = Y.PartNumber) As PontaPA
,(Select EstadoPonta from GHT_DATAWAREHOUSE.DW.DimCadContrato As Y (NOLOCK) Where Y.TipoContrato = 'Contrato Vale' And Y.EstadoPonta = 'MG' And E.PartNumber = Y.PartNumber) As PontaMG
,(Select Ponta from GHT_DATAWAREHOUSE.DW.DimCadContrato As Y (NOLOCK) Where TipoContrato = 'Contrato Vale' And Registroversao = 'Atual' And E.PartNumber = Y.PartNumber) As Ponta

From CTE As E (NOLOCK)
inner Join Ght_DataWarehouse.Dw.DimCadFicha As P (nolock) On (E.PartNumber= P.PartNumber and E.Fornecedor = P.NomeFornecedor) 
Left join Ght_DataWarehouse.Dw.DimCadFornecedor As F (nolock) on (P.NomeFornecedor = F.FornCod)
Left Join Ght_DataWarehouse.Dw.DimCadLinha As L (nolock) on (P.CodLinha = L.CodLin)
Left Join Ght_DataWarehouse.Dw.DimCadMarca As M (Nolock) on (P.CodMarca = M.CodMarc)
Left join Ght_DataWarehouse.Dw.DimCadFamilia As A (nolock) On (P.CodFamilia = A.CodFam)
Left Join (Select CodFilial, Produto, Fornecedor, Periodo, Sum(CmvDre) As CustoVenda , Sum(FatuComSt) As Fatura, Sum(FatuMkp) As MKP  From Ght_DataWarehouse.Dw.DimMargemGerencial Where Cagtegoria = 'FATURAMENTO'group by CodFilial, Produto, Fornecedor, Periodo)
As G On (E.PartNumber = G.Produto And E.Fornecedor = G.FORNECEDOR And E.CodEmpresa = G.CodFilial And E.PeriodoMov = G.PERIODO)
Left Join Ght_DataWarehouse.Dw.DimClassfProduto As B (nolock) On (P.prodClassif = B.ClassCod)
Left Join Ght_DataWarehouse.Dw.DimGrupoProduto As U (nolock) On(P.ProdGrupo = U.CodGrupo)
Left Join GHT_DATAWAREHOUSE.DW.DimCadEmpresa As S (NoLock) On (E.CodEmpresa = S.CodEmpresa)
Left Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NoLock) On (E.PeriodoMov = T.DataPeriodo)
Left Join (Select Idfilial, Produto, Fornecedor, PeriodoMov, SUM(qtde) QtdeVendida, SUM(FatComImp) FaturadoVale, SUM(CustoVenda) CustoVendaVale from GHT_DATAWAREHOUSE.DW.DimContratoVale (NOLOCK) Where Tipo = 'Faturado' And Gerencia = 'VALE MINERAÇÃO' Group by Idfilial, Produto, Fornecedor, PeriodoMov) As X  On (E.PartNumber = X.Produto And E.Fornecedor = X.Fornecedor And E.PeriodoMov = X.PeriodoMov And E.CodEmpresa = X.IdFilial)
Left Join (Select Idfilial, Produto, Fornecedor, PeriodoMov, SUM(qtde) QtdeVendida, SUM(FatComImp) FaturadoVale, SUM(CustoVenda) CustoVendaVale from GHT_DATAWAREHOUSE.DW.DimContratoVale (NOLOCK) Where Tipo = 'Pendente' And Gerencia = 'VALE MINERAÇÃO' Group by Idfilial, Produto, Fornecedor, PeriodoMov) As X1  On (E.PartNumber = X1.Produto And E.Fornecedor = X1.Fornecedor And E.PeriodoMov = X1.PeriodoMov And E.CodEmpresa = X1.IdFilial)
Left Join GHT_DATAWAREHOUSE.DW.DimCadDeParaPN (NOLOCK) As Z On (E.PartNumber = Z.PartNumberAnt)
Left Join GHT_DATAWAREHOUSE.DW.DimFichaEmTransito (NOLOCK) As H On (E.CodEmpresa = H.CodEmpresa And E.PartNumber = H.PartNumber And E.Fornecedor = H.Fornecedor And E.PeriodoMov = @DataPeriodo)
Left Join (Select CodEmpresa, PartNumber, Fornecedor, MAX(Periodo) Periodo, SUM(Saldo) Saldo from GHT_DATAWAREHOUSE.DW.DimEmTransito  Group by CodEmpresa, PartNumber, Fornecedor) As H1 On (E.CodEmpresa = H1.CodEmpresa And E.PartNumber = H1.PartNumber And E.Fornecedor = H1.Fornecedor And E.PeriodoMov &lt;= H1.Periodo)
Left Join (Select M.Produto, Count(Distinct T.DataPeriodo) MesesVendas from GHT_DATAWAREHOUSE.DW.DimTempoMes as T Inner Join GHT_DATAWAREHOUSE.DW.DimMargemGerencial As M On (T.DataPeriodo = M.Periodo) Where IDSKTTempMes Between @DataMaxima - 11 And @DataMaxima Group by M.Produto) As W On (E.PartNumber = W.Produto And E.PeriodoMov = @DataPeriodo)
Left Join (Select M.CodFilial, M.Produto, Count(Distinct T.DataPeriodo) MesesVendas from GHT_DATAWAREHOUSE.DW.DimTempoMes as T Inner Join GHT_DATAWAREHOUSE.DW.DimMargemGerencial As M On (T.DataPeriodo = M.Periodo) Where IDSKTTempMes Between @DataMaxima - 11 And @DataMaxima Group by M.CodFilial, M.Produto) As W1 On (E.CodEmpresa = W1.CodFilial And E.PartNumber = W1.Produto And E.PeriodoMov = @DataPeriodo)
Left join GHT_DATAWAREHOUSE.DW.DimProdutoVendaGht As D (NOLOCK) On (E.CodEmpresa = D.CodEmpresa And E.PartNumber = D.PartNumber And E.Fornecedor = D.Fornecedor And E.PeriodoMov = D.Periodo)
Left join GHT_DATAWAREHOUSE.DW.DimProdutoVendaVale As I (NOLOCK) On (E.CodEmpresa = I.CodEmpresa And E.PartNumber = I.PartNumber And E.Fornecedor = I.Fornecedor And E.PeriodoMov = I.Periodo)
Left Join (Select D2.CodEmpresa, D2.PartNumber, D2.Fornecedor, Sum(D2.Qtde) venda, Count(Distinct D2.Periodo) Meses From GHT_DATAWAREHOUSE.DW.DimProdutoVendaGHT (NOLOCK) As D2 Left Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NoLock) On (D2.Periodo = T.DataPeriodo) Where T.IDSKTTempMes Between @DataMaxima - 11 And @DataMaxima Group by D2.CodEmpresa, D2.PartNumber, D2.Fornecedor) As D1 On (E.CodEmpresa = D1.CodEmpresa And E.PartNumber = D1.PartNumber And E.Fornecedor = D1.Fornecedor And E.PeriodoMov = @DataPeriodo)
Left Join (Select D4.CodEmpresa, D4.PartNumber, D4.Fornecedor, Sum(D4.Qtde) venda, Count(Distinct D4.Periodo) Meses From GHT_DATAWAREHOUSE.DW.DimProdutoVendaVale (NOLOCK) As D4 Left Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NoLock) On (D4.Periodo = T.DataPeriodo) Where T.IDSKTTempMes Between @DataMaxima - 11 And @DataMaxima Group by D4.CodEmpresa, D4.PartNumber, D4.Fornecedor) As D3 On (E.CodEmpresa = D3.CodEmpresa And E.PartNumber = D3.PartNumber And E.Fornecedor = D3.Fornecedor And E.PeriodoMov = @DataPeriodo)
Left join (Select CodEmpresa, PartNumber, NomeFornecedor, Sum(Qtde) Qtd From GHT_DATAWAREHOUSE.DW.DimReservaEstoque (NOLOCK) Group by CodEmpresa, PartNumber, NomeFornecedor) As Q On (E.CodEmpresa = Q.CodEmpresa And E.PartNumber = Q.PartNumber And E.Fornecedor = Q.NomeFornecedor And E.PeriodoMov = @DataPeriodo)
Left Join (Select CodEmpresa, PartNumber, NomeFornecedor, Sum(QtdPedido) Qtd from GHT_DATAWAREHOUSE.DW.DimPedidoPendenteOn (Nolock) Group by CodEmpresa, PartNumber, NomeFornecedor) As J On (E.CodEmpresa = J.CodEmpresa And E.PartNumber = J.PartNumber And E.Fornecedor = J.NomeFornecedor And E.PeriodoMov = @DataPeriodo)
Left Join (Select CodEmpresa, PartNumber, NomeFornecedor, Sum(Qtde) QtdOc, MAX(TotalOC) TotalOc, MAX(DtOcentrega) DtOcEntrega from GHT_DATAWAREHOUSE.DW.DimOrdemCompra_NL Group by  CodEmpresa, PartNumber, NomeFornecedor) As O On (E.CodEmpresa = O.CodEmpresa And E.PartNumber = O.PartNumber And E.Fornecedor = O.NomeFornecedor And E.PeriodoMov = @DataPeriodo)
Where T.IDSKTTempMes Between @DataMaxima - 11 And @DataMaxima</property>
