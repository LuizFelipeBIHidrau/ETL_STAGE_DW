                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">with cte as(
Select 
F.IdEmpresa As CodEmpresa,
P.Prodnumpeca , 
P.NomeFornecedor,
Movdata As PeriodoMov,
--Sum(Faturamento) As Fatura,
F.QtdEstoque
from Comercial_ST.[Stage].[FatoEstoque] As F
Left join  Comercial_St.Stage.Cadproduto As P ON ( F.Idproduto = P.CodProd And P.PeriodoMov = '202303')
Where F.MovData = '202303' And (F.custoEstoque &gt; 0 Or F.Faturamento &gt; 0 )
--Group by Movdata
)

select P.* 
From DataMart_Comercial.dm.DImCadProduto As P
Inner join Cte As E ON (P.ProdnumPeca = E.ProdNumPeca And P.NomeFornecedor = E.NomeFornecedor )
