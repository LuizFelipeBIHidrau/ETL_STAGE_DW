      ,P.ProdItemRoxo
      ,P.ProdCustoSemImposto
      ,P.ProdAtivo
      ,P.ProdFrequencia
      ,P.ProdLeadTime
  FROM GHT_DATAWAREHOUSE.DW.DimCadProduto As P
Inner Join Ght_Datawarehouse.DW.DimTempoMes As T ON (P.PeriodoMov = T.DataPeriodo)
Left join (Select Distinct ProdNumPeca, FornecedorNome From GHT_STAGE.ST.EstoqueFilial Where QuantEstoque &gt; 0 And PeriodoMov = '202304') As E ON (P.ProdNumPeca = E.ProdNumPeca And P.NomeFornecedor = E.FornecedorNome)
Left Join (Select Distinct Produto, Fornecedor From GHT_DATAWAREHOUSE.DW.DimMargemGerencial Where Periodo = '202304')  As M ON (P.ProdNumPeca = M.Produto And P.NomeFornecedor = M.Fornecedor)
Where P.PeriodoMov = '202305' And (E.ProdNumPeca is not null Or M.Produto Is Not Null)</property>
