                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @MaxData Varchar(6)
  Select @MaxData = (Select Max(PeriodoMov) From GHT_STAGE.ST.EstoqueFilial) 

;With CTE AS 
(
  Select Distinct 
 -- F.CodEmpresa,
  F.ProdNumPeca, 
  F.FornecedorNome,
  F.PeriodoMov
  --F.QuantEstoque
  From GHT_STAGE.ST.EstoqueFilial (NOLOCK) As F
  Left Join GHT_DATAWAREHOUSE.DW.DimCadProduto (NOLOCK)  As P On (F.ProdNumPeca = P.ProdNumPeca And F.FornecedorNome = P.NomeFornecedor And F.PeriodoMov = P.PeriodoMov)
  Left Join GHT_DATAWAREHOUSE.DW.DimMargemGerencial (NOLOCK) As M On (F.CodEmpresa = M.CodFilial And F.ProdNumPeca = M.Produto And F.FornecedorNome = M.Fornecedor And F.PeriodoMov = M.Periodo )
  Where F.PeriodoMov = @MaxData And (F.QuantEstoque &gt; 0 OR M.FatuComImposto &gt; 0)
)

SELECT DISTINCT
	  P.IDSKProduto
      ,P.CodProd
      ,P.ProdNumPeca
      ,P.NomeFornecedor
      ,P.ProdNome
      --,'202304' As PeriodoMov
	  ,P.PeriodoMov
      ,P.ProdFamilia
      ,P.ProdMarca
      ,P.ProdLinha
      ,P.ProdClassFiscal
      ,P.ProdUnidMedida
      ,P.ProdPeso
      ,P.ProdStatusCor
      ,P.ProdRep
      ,P.ProdItemRoxo
      ,P.ProdCustoSemImposto
      ,P.ProdAtivo
      ,P.ProdFrequencia
      ,P.ProdLeadTime
  FROM GHT_DATAWAREHOUSE.DW.DimCadProduto As P
  Inner Join CTE As E On (P.ProdNumPeca = E.ProdNumPeca And P.NomeFornecedor = E.FornecedorNome And P.PeriodoMov = E.PeriodoMov)</property>
