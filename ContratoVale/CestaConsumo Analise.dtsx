                <component
                  refId="Package\CestaConsumo Stage para DW\DW DimCestaConsumo\Stage CestaConsumo"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="Origem OLE DB;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; Todos os Direitos Reservados; http://www.microsoft.com/sql/support;7"
                  description="Origem OLE DB"
                  name="Stage CestaConsumo"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="O tempo em segundos antes que um comando expire. Zero indica uma expiração infinita."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Especifica o nome do objeto de banco de dados usado para abrir um conjunto de linhas."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Especifica a variável que contém o nome do objeto de banco de dados usado para abrir um conjunto de linhas."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="O comando SQL a ser executado."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">;With Cte As (
  Select 
	Coalesce(A.PartNumberNew, E.ProdNumPeca) As PartNumber,
	E.PeriodoMov,
	Sum(E.QuantEstoque) As QtdeEst,
	ISNull((Select Sum(E1.QuantEstoque) From GHT_STAGE.ST.EstoqueFilial As E1 Where E1.UF = 'SP' And E.ProdNumPeca = E1.ProdNumPeca And E.PeriodoMov = E1.PeriodoMov),0) As Est_SP,
	ISNull((Select Sum(E1.QuantEstoque) From GHT_STAGE.ST.EstoqueFilial As E1 Where E1.UF = 'MG' And E.ProdNumPeca = E1.ProdNumPeca And E.PeriodoMov = E1.PeriodoMov),0) As Est_MG,
	ISNull((Select Sum(E1.QuantEstoque) From GHT_STAGE.ST.EstoqueFilial As E1 Where E1.UF = 'PA' And E.ProdNumPeca = E1.ProdNumPeca And E.PeriodoMov = E1.PeriodoMov),0) As Est_PA

from GHT_STAGE.ST.EstoqueFilial As E
Inner Join GHT_STAGE.ST.CadEmpresa As P On (E.CodEmpresa = P.CodEmpresa)
Left Join ST.CadDeParaPN As A (NOLOCk) On (E.ProdNumPeca = A.PartNumberAnt)
Where E.ProdNumPeca is not null 
Group by A.PartNumberNew, E.ProdNumPeca, E.PeriodoMov
)

SELECT C.PartNumber
      ,C.Descricao
      ,C.Mina
      ,C.Planta
      ,C.DataMov
      ,CONVERT(char(6), C.DataMov, 112) As Periodo
	  ,CONVERT(char(4), C.DataMov, 112) As Ano
	  ,T.MesBarra01 As MesAbrev
      ,C.Região
      ,C.Estado
      ,C.OemSpot
      ,C.PrecoGht
	  ,M.Qtde As QtdeVendVale
	  ,M.VlrVendido As FaturaVale
      ,E.Est_SP As EstoqueSP
      ,E.Est_PA As EstoquePA
      ,E.Est_MG As EstoqueMG
      ,SUm(C.ConsumoOEM) As ConsumoOEM
      ,C.ConsumoPN
      ,C.Cnpj
      ,D.CodMina
	  ,V.ItemCampanha As PerfilPonta
  FROM ST.AnaliseCestaConsumo As C (NOLOCK)
  Left Join (select Distinct PlantaCesta, CodMina from ST.DeparaProjetos) As D On (C.Planta = D.PlantaCesta)
  Left Join (Select Distinct PartyNumber,ItemCampanha From GHT_DATAWAREHOUSE.DW.DimCadContrato Where TipoContrato = 'Contrato Vale' And RegistroVersao = 'Atual') As V On (C.PartNumber = V.PartyNumber)
  Left Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T On (CONVERT(char(6), C.DataMov, 112) = T.DataPeriodo)
  Left Join Cte As E On (C.PartNumber = E.PartNumber And CONVERT(char(6), C.DataMov, 112) = E.PeriodoMov)  
  Left Join (Select Produto, PeriodoMov, Sum(Qtde) Qtde, Sum(FatComImp) VlrVendido from GHT_DATAWAREHOUSE.DW.DimContratoVale Where Tipo = 'Faturado' Group by Produto, PeriodoMov) As M On (C.PartNumber = M.Produto And CONVERT(char(6), C.DataMov, 112) = M.PeriodoMov)
  Group by 
       C.PartNumber
      ,C.Descricao
      ,C.Mina
      ,C.Planta
      ,C.DataMov
      ,CONVERT(char(6), C.DataMov, 112) 
	  ,CONVERT(char(4), C.DataMov, 112)
	  ,T.MesBarra01
      ,C.Região
      ,C.Estado
      ,C.OemSpot
      ,C.PrecoGht
	  ,M.Qtde
	  ,M.VlrVendido
      ,E.Est_SP
      ,E.Est_PA      
      ,E.Est_MG      
      ,C.ConsumoPN
	  ,C.Cnpj
	  ,D.CodMina
	  ,V.ItemCampanha</property>
