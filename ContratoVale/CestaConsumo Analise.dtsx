                <component
                  refId="Package\Cesta Consumo Analise Base em Procedure\carga Procedure para Stage CestaConsumo Analise\Procedure STP SMP CESTACONSUMOANALISE"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="Origem OLE DB;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation;  Todos os Direitos Reservados; http://www.microsoft.com/sql/support;7"
                  description="Origem OLE DB"
                  name="Procedure STP SMP CESTACONSUMOANALISE"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="O tempo em segundos antes que um comando expire. Zero indica uma expiração infinita."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Especifica o nome do objeto de banco de dados usado para abrir um conjunto de linhas."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Especifica a variável que contém o nome do objeto de banco de dados usado para abrir um conjunto de linhas."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="O comando SQL a ser executado."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
	YEAR(CV.mes_ano)AS ANO
	,case	when len(convert(varchar,month(cv.mes_ano))) =1 then '0'+ convert(varchar,month(cv.mes_ano))
				else convert(varchar,month(cv.mes_ano)) end 								
					AS MES
	,cv.MES_ANO
	,sum(cv.qtde)	AS CONSUMO_OEM
	,cv.planta
	,cv.PART_NUMBER
	,'' as Codigo_Vale
	,cv.OEM_SPOT
	,ltrim(rtrim(MINA.CLI_APELIDO))	AS MINA
	,ltrim(rtrim(FED.UFE_REGIAO))	AS REGIAO
	,ltrim(rtrim(MINA.CLI_FEST))	AS ESTADO
	,MINA.CLI_CGC AS CNPJ
	,c.DESCRICAO
	,ct.PRECO as PRECO_GHT
	,ESTQ.ESTQ_SP
	,ESTQ.ESTQ_PA
	,ESTQ.ESTQ_MG
	,null as QTDE_PN_GHT
	,null as QTDE_PN_VALE
	,CT1.Contrato
	,CT2.MARCA_SPR
	,CT5.MARCA
	,CT3.LINHA
	,GRP.GRUPO
	--Chamado 105978 - Solicitado pela Jessica que traga a familia da nova tabela de familia vale criada exclusivamente para o Plano de Ação
	--,CT4.FAMILIA
	,COALESCE(VF.FAMILIA,CT4.FAMILIA) as FAMILIA
	,'' As PN_Contrato
	,Aplica.Aplicacao
	,GIRO.COR
from 
	CESTA_VALE CV (NOLOCK)

LEFT JOIN 
		VALE_FAMILIA vf on
		CV.PART_NUMBER = vf.PART_NUMBER

inner join (select distinct
			vi.CT_PART_NUMBER 
		from 
			VALE_CONTRATO_itens VI (nolock)
		inner join VALE_CONTRATO vc (nolock) on
			vc.CT_ID = vi.CT_ID_CONTRATO AND
			ISNULL(VC.CT_SUSPENSO,'') &lt;&gt; 'S') contrato on contrato.CT_PART_NUMBER = cv.PART_NUMBER
left join CPACLIE MINA (nolock) on 
			planta in (select value from STRING_SPLIT(MINA.cli_nroplanta,',')) AND MINA.cli_situ='A'		
left join SIAUFED FED (nolock) on	
			MINA.CLI_FEST = FED.UFE_CODI  

LEFT OUTER JOIN  dbo.siapdpa (nolock) S on 
								s.PDP_PANT=Cv.PART_NUMBER 									

left join (select max(DESCRICAO) AS DESCRICAO,PART_NUMBER 
			FROM CESTA_VALE (NOLOCK)
			GROUP BY PART_NUMBER ) C on c.PART_NUMBER = cv.PART_NUMBER 
left join (select min(ct_preco) as PRECO, CT_PART_NUMBER 
				from VALE_CONTRATO_ITENS (NOLOCK)
				inner join VALE_CONTRATO vc (nolock) on
							vc.CT_ID = CT_ID_CONTRATO AND
							VC.CT_DESCRICAO like '%MG%' AND
							ISNULL(VC.CT_SUSPENSO,'') &lt;&gt; 'S'				
				group by ct_part_number) ct on ct.CT_PART_NUMBER=cv.PART_NUMBER
LEFT OUTER JOIN (select
					sum(HIDRAU+HF_100+HTMOVEIS+UNIONP) as ESTQ_SP,
					--chamado 70904 sum(PARA+TROMBETAS+MARABA+XINGU) as ESTQ_PA,
					sum(PARA) as ESTQ_PA,
					--chamado 70904 sum(CONTAGEM+DIVINOPOLIS+UBERABA) as ESTQ_MG,
					sum(CONTAGEM) as ESTQ_MG,
					produto from movimento..estoqueconsolidado (nolock)
					group by produto
					) 
					ESTQ ON ESTQ.produto = Cv.PART_NUMBER 
-- Grupo  --
		LEFT OUTER JOIN (select PRODUTO,GRUPO FROM(
						SELECT DISTINCT
						P.PRO_CODI AS PRODUTO,
						CASE WHEN L.LIN_DESC IN ('MERCEDES BENZ','SCANIA','VOLVO TREVISO','VOLKSWAGEM','MWM','IMPLEMENTOS','IVECO') THEN 'CAMINHÕES LEVES'
							 WHEN (F.FAM_DESC IN ('GHT SISTEMAS','AUTOMACAO','MANUT PREDITIVA','FERRAMENTAS','EQUIP SEGURANCA') OR S.CLASDESCRICAO IN ('EQUIP DE SEGURANCA')) THEN 'SISTEMAS'
							 WHEN (F.FAM_DESC IN ('ESCAV ELETR PECAS','ESCAVADEIRA ELETRICA','ESCAVADEIRA') OR L.LIN_DESC IN ('ATLAS COPCO_GP','KOMATSU_GP','LETORNEAU','LETOURNEAU','BUCYRUS','P&amp;H')) THEN 'EQUIP GRANDE PORTE'
							ELSE ''
						END AS GRUPO
						FROM DEPCATUMBI..SIAPROD P (NOLOCK)
						LEFT JOIN DEPCATUMBI..SIAFAMI F WITH(NOLOCK) ON P.PRO_FAMI = F.FAM_CODI
						LEFT JOIN DEPCATUMBI..SIALINH L WITH(NOLOCK) ON P.PRO_LINH = L.LIN_CODI
						LEFT JOIN DEPCATUMBI..CLASSIFPRODUTO S WITH(NOLOCK) ON S.CLASCODIGO = P.PRO_CLASSIFICACAO
						WHERE ISNULL(P.PRO_EXCL,'N') &lt;&gt; 'S')GRP_ where ISNULL(GRP_.GRUPO,'')&lt;&gt;'') GRP on GRP.PRODUTO=Cv.PART_NUMBER


LEFT OUTER JOIN (SELECT distinct Main.CT_PART_NUMBER,
						LEFT(Main.Students,Len(Main.Students)-1) As Contrato
					FROM
						(
							SELECT DISTINCT ST2.CT_PART_NUMBER , 
								(
									SELECT DISTINCT ST1.CT_DESCRICAO + ',' AS [text()]
									FROM dbo.vale_contrato ST1 (NOLOCK)
									inner join VALE_CONTRATO_ITENS vci (nolock) on
									 vci.CT_ID_CONTRATO = ST1.CT_ID 								
									WHERE  st2.CT_PART_NUMBER=vci.CT_PART_NUMBER AND				 
									 ISNULL(ST1.CT_SUSPENSO,'') &lt;&gt; 'S'				
									FOR XML PATH ('')
								) [Students]
							FROM dbo.VALE_CONTRATO_ITENS ST2 (NOLOCK)
							inner join VALE_CONTRATO VC (nolock)
							on
									 VC.CT_ID = ST2.CT_ID_CONTRATO
							where ISNULL(VC.CT_SUSPENSO,'') &lt;&gt; 'S'		
		
					) [Main] ) CT1 on CT1.CT_PART_NUMBER=CV.PART_NUMBER

LEFT OUTER JOIN	(SELECT distinct Main.Produto,
					   LEFT(Main.Students,Len(Main.Students)-1) As MARCA_SPR
					FROM
						(
							SELECT DISTINCT ST2.Produto , 
								(
									SELECT DISTINCT ST1.FORNECEDOR + ',' AS [text()]
									FROM dbo.ItensHomologados ST1	(NOLOCK)								
									WHERE  st2.Produto=st1.Produto and
									st1.Fornecedor &lt;&gt;'' and
									st1.Fornecedor is not null
									FOR XML PATH ('')
								) [Students]
							FROM dbo.ItensHomologados ST2 (NOLOCK)
						) [Main] ) CT2 on CT2.Produto=cv.PART_NUMBER

LEFT OUTER JOIN (SELECT distinct Main.PRO_CODI,
       LEFT(Main.Students,Len(Main.Students)-1) As LINHA
FROM
    (
        SELECT DISTINCT ST2.PRO_CODI , 
            (
                SELECT DISTINCT ST1.LIN_DESC + ',' AS [text()]
                 from SIALINH ST1 (nolock) 
					INNER JOIN SIAPROD PROD (nolock) ON
								ST1.LIN_CODI = PROD.PRO_LINH					
			 where   
				  PROD.PRO_EXCL &lt;&gt; 'S' AND
				  PROD.PRO_CODI = ST2.Pro_Codi
                FOR XML PATH ('')
            ) [Students]
        FROM dbo.SIAPROD ST2 (NOLOCK)
		WHERE ST2.PRO_EXCL &lt;&gt; 'S'
		
		
    ) [Main] ) CT3 on CT3.PRO_CODI=CV.PART_NUMBER


LEFT OUTER JOIN (SELECT distinct Main.PRO_CODI,
						   LEFT(Main.Students,Len(Main.Students)-1) As FAMILIA
					FROM
						(
							SELECT DISTINCT ST2.PRO_CODI , 
								(
									SELECT DISTINCT ST1.FAM_DESC + ',' AS [text()]
									 from SIAFAMI ST1 (nolock) 
										INNER JOIN SIAPROD PROD (nolock) ON
													ST1.FAM_CODI = PROD.PRO_FAMI					
								 where   
									  PROD.PRO_EXCL &lt;&gt; 'S' AND
									  PROD.PRO_CODI = ST2.Pro_Codi
									FOR XML PATH ('')
								) [Students]
							FROM dbo.SIAPROD ST2 (NOLOCK)
							WHERE ST2.PRO_EXCL &lt;&gt; 'S'
							
						) [Main] ) CT4 on CT4.PRO_CODI = cv.PART_NUMBER


LEFT OUTER JOIN (SELECT distinct Main.PRO_CODI,
       LEFT(Main.Students,Len(Main.Students)-1) As MARCA
FROM
    (
        SELECT DISTINCT ST2.PRO_CODI , 
            (
                SELECT DISTINCT rtrim(ST1.MAR_DESC) + ',' AS [text()]
                 from SIAMARC ST1 (nolock) 
					INNER JOIN SIAPROD PROD (nolock) ON
								ST1.MAR_CODI = PROD.PRO_MARC					
			 where   
				  PROD.PRO_EXCL &lt;&gt; 'S' AND
				  PROD.PRO_CODI = ST2.Pro_Codi
                FOR XML PATH ('')
            ) [Students]
        FROM dbo.SIAPROD ST2 (NOLOCK)
		WHERE ST2.PRO_EXCL &lt;&gt; 'S'
		
		
    ) [Main] ) CT5 on CT5.PRO_CODI=CV.PART_NUMBER

Outer Apply (Select distinct cv1.part_number,
			STUFF(
					(select distinct ', ' + ltrim(rtrim(maq_codi)) from
						(select Maq_codi,ItAnalise_codi   from maquinas..INItemAnaliseItens A (nolock),maquinas..INItemAnalise B (nolock),maquinas..INMaquina (nolock)
						where a.itanaliseit_id = b.ItAnalise_id 
						and ItAnalise_Maquina = maq_id 
						UNION
						select Maq_codi,ItAnalise_codi  from maquinassto..INItemAnaliseItens A (nolock) ,maquinassto..INItemAnalise B (nolock) ,maquinassto..INMaquina (nolock)
						where a.itanaliseit_id = b.ItAnalise_id 
						and ItAnalise_Maquina = maq_id) apli 
						where  ItAnalise_codi=cv1.PART_NUMBER
						FOR XML PATH('')
					   ),1,1,''
					   ) as Aplicacao
			from CESTA_VALE cv1 (nolock)
			where cv1.part_number = cv.PART_NUMBER ) Aplica

LEFT OUTER JOIN  (select distinct
					pro_codi					
					,(CASE 
					WHEN B.PRO_SREP = 'S' THEN 'Preto' -- sem giro um ano
					WHEN B.PRO_EXCESSOESTOQUE = 2 THEN 'Amarelo' --Sem giro 3 a 6 meses					
					END) AS COR
					from depcatumbi..siaprod b (nolock)
					inner join (SELECT DISTINCT ST1.Produto, ST1.FORNECEDOR 
					FROM dbo.ItensHomologados ST1	(NOLOCK)) fornec on fornec.Fornecedor = pro_forn and fornec.Produto=	b.PRO_CODI
					where 
						(B.PRO_SREP = 'S' or B.PRO_EXCESSOESTOQUE = 2)
					and isnull(pro_excl,'N') &lt;&gt; 'S'
					group by b.pro_codi,b.PRO_SREP, b.Pro_ExcessoEstoque--,b.PRO_FORN
					) 
					GIRO ON GIRO.PRO_CODI =  Cv.PART_NUMBER 
where cv.mes_ano &gt;='2021-01-01 00:00:00.000' and cv.MES_ANO &lt;= Getdate() and cv.qtde&gt;0 and isnull(MINA.CLI_CGC,'')&lt;&gt;''
group by cv.MES_ANO
		,cv.PLANTA
		,cv.PART_NUMBER		
		,cv.OEM_SPOT
		,mina.CLI_APELIDO
		,fed.Ufe_regiao
		,mina.CLI_FEST
		,c.DESCRICAO
		,ct.PRECO
		,ESTQ.ESTQ_SP
		,ESTQ.ESTQ_PA
		,ESTQ.ESTQ_MG
		,CT1.Contrato
		,CT2.MARCA_SPR
		,CT3.LINHA
		,CT4.FAMILIA
		,MINA.CLI_CGC
		,CT5.MARCA
		,GIRO.COR
		,GRP.GRUPO
		,Aplica.Aplicacao
		,VF.FAMILIA</property>
