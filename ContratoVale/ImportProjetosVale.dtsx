                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">;With Estoque As (
  Select 
	E.ProdNumPeca,
	E.PeriodoMov,
	Sum(E.QuantEstoque) As QtdeEst,
	ISNull((Select Sum(E1.QuantEstoque) From GHT_STAGE.ST.EstoqueFilial As E1 Where E1.UF = 'SP' And E.ProdNumPeca = E1.ProdNumPeca And E.PeriodoMov = E1.PeriodoMov),0) As Est_SP,
	ISNull((Select Sum(E1.QuantEstoque) From GHT_STAGE.ST.EstoqueFilial As E1 Where E1.UF = 'MG' And E.ProdNumPeca = E1.ProdNumPeca And E.PeriodoMov = E1.PeriodoMov),0) As Est_MG,
	ISNull((Select Sum(E1.QuantEstoque) From GHT_STAGE.ST.EstoqueFilial As E1 Where E1.UF = 'PA' And E.ProdNumPeca = E1.ProdNumPeca And E.PeriodoMov = E1.PeriodoMov),0) As Est_PA

from GHT_STAGE.ST.EstoqueFilial As E
Where E.ProdNumPeca is not null 
Group by E.ProdNumPeca, E.PeriodoMov
)

SELECT Distinct 
       V.PartNumber As PN
	  ,IsNull(Coalesce(C.Descricao, P1.ProdNome), 'Sem Descrição') As Descricao
      ,V.NomeProjeto As Projeto
	  ,D.Mina As Unidade
   ,CAST(STUFF((Select Distinct ' / '+ C1.NumContrato
	From (Select * From GHT_DATAWAREHOUSE.DW.DimCadContrato Where TipoContrato = 'Contrato Vale') As C1 
	Where C1.PartyNumber = V.PartNumber Or C1.PartyNumber = V.PartNumber 
	For XML PATH ('')), 1,2, '') As varchar(100)) As Contratos
   ,CAST(STUFF((Select Distinct ' / '+ C1.DescricaoCapa
	From (Select * From GHT_DATAWAREHOUSE.DW.DimCadContrato Where TipoContrato = 'Contrato Vale') As C1 
	Where C1.PartyNumber = V.PartNumber Or C1.PartyNumber = V.PartNumber 
	For XML PATH ('')), 1,2, '') As varchar(100)) As DescContratos
      ,V.Cor
	  ,A.DataRecebList
	  ,A.Observacao
	  ,A.Situacao
	  ,A.VrResponsavel
	  ,A.AnaliseStatus
	  ,A.Ph As PrecoHidrau
