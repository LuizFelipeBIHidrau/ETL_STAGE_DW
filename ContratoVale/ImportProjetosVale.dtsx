	  ,S.DtPa As DtEntregaPA			  
	  ,S.DtMg DtEntregaMG			  
	  ,R.Registro As Registro
	  ,W.MesNum As QtdMeses
	  ,SUM(B.ConsumoOEM) Over(Partition by V.PartNumber, SUBSTRING(V.Periodo,1,4) Order By V.periodo) As TotalOEM
	  ,D.UF As UFMina   

  FROM ST.ProjetoVale As V (Nolock)
  Inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T (NOLOCK) ON (V.Periodo = T.DataPeriodo)  
  Left Join (select PartNumber, Sum(SaltoTotal)Tl, Sum(SaldoSP_Transito) S_SP, Sum(SaldoPA_Transito) S_PA, Sum(SaldoMG_Transito) S_MG, Max(DtEntregaSP) DtSp, Max(DtEntregaPA) DtPa, Max(DtEntregaMG) DtMg  from GHT_DATAWAREHOUSE.DW.DimEmTransitoPN Group by PartNumber) As S ON (V.PartNumber = S.PartNumber)
  Left Join Estoque As Q ON (V.PartNumber = Q.ProdNumPeca And V.Periodo = Q.PeriodoMov) 
  Left Join (Select Distinct ProdNumPeca,MAX(ProdNome) ProdNome From GHT_DATAWAREHOUSE.DW.DimCadProduto Where PeriodoMov = (Select Max(PeriodoMov) From st.CadProduto) Group by ProdNumPeca) As P1 On (V.PartNumber = P1.ProdNumPeca)
  Left Join (Select Distinct PartNumber, Max(DescricaoItem) As Descricao, ItemCampanha,Preco As PrecoAtual From GHT_DATAWAREHOUSE.DW.DimCadContrato Where TipoContrato = 'Contrato Vale' And RegistroVersao = 'Atual' Group by PartNumber, ItemCampanha, Preco) As C On (V.PartNumber = C.PartNumber)
  Left join (Select Distinct Partnumber, DataRecebList, Observacao, Situacao, VrResponsavel, AnaliseStatus, Max(PrecoHidrau) As Ph, Max(PrecoAtual) As Pa from GHT_DATAWAREHOUSE.DW.DimAnaliseItemContrato Where RegistroVersao = 'Atual'group by PartNumber, DataRecebList, Observacao, Situacao, VrResponsavel, AnaliseStatus) As A On (V.PartNumber = A.PartNumber)
  Left Join (Select Distinct CodMina,UF,Regiao,Mina from ST.DeparaProjetos) As D On (V.Mina = D.CodMina)
  Left Join ( Select PartNumber, Periodo, PrecoGht,CodMina_BI, Sum(ConsumoOEM) As ConsumoOEM from Ght_DataWareHouse.DW.DimCestaConsumo Group by PartNumber, Periodo, PrecoGht,CodMina_BI) As B On (V.PartNumber = B.PartNumber and V.Periodo = B.Periodo And V.Mina = B.CodMina_BI)
  Left join (Select Produto, PeriodoMov, Sum(Qtde) As QtdeVale, SUM(FatComimp) As FaturaVale from GHT_DATAWAREHOUSE.DW.DimContratoVale Where Tipo = 'Faturado' Group by Produto, PeriodoMov) As M On(V.PartNumber = M.Produto And V.Periodo = M.PeriodoMov)
  Left Join (select Distinct PartNumber, COUNT(Distinct Periodo) MesNum, SUBSTRING(Periodo,1,4) Ano FROM ST.ProjetoVale Group by PartNumber, SUBSTRING(Periodo,1,4)) AS W ON (V.PartNumber = W.PartNumber And T.Ano = W.Ano And V.Periodo = (Select Distinct MAX(Periodo) From St.ProjetoVale))
Left Join (
Select V.PartNumber, V.NomeProjeto, V.Periodo,
COUNT(V.PartNumber) Over(Partition by V.PartNumber, V.Periodo Order by V.Periodo Rows Between Unbounded Preceding And Current Row) As Registro
from ST.ProjetoVale As V) As R ON (V.PartNumber = R.PartNumber And V.NomeProjeto = R.NomeProjeto And V.Periodo = R.Periodo)</property>
