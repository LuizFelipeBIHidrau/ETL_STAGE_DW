	  ,T.Ano
	  ,Q.QtdeEst As Estoque
	  ,Q.Est_SP As Est_SP            
	  ,Q.Est_PA As Est_PA 			 
	  ,Q.Est_MG As Est_MG				 
	  ,S.S_SP As SaldoSP
	  ,S.S_PA As SaldoPA
	  ,S.S_MG As SaldoMG
	  ,B.ConsumoOEM          
	  ,D.Regiao
	  ,C.ItemCampanha As PerfilPonta
