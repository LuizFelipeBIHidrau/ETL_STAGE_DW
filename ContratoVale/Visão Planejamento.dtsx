from(
		select distinct 
			part_number, 
			'' as pro_desc,
			MES_ANO,
			SUM(QTDE) as QTDE 
		from cesta_vale (NOLOCK)
		where qtde&gt;0 and
			mes_ano&gt;=@DataInicio and
			mes_ano&lt;=@DataFimCesta
		group by part_number,MES_ANO
		union all 
		select  
			pro_codi,			
			max(pro_desc) as pro_desc,
			Convert(datetime,@DataInicio) as MES_ANO,
			0
		from
			SIAPROD  (NOLOCK)
		WHERE	
			ISNULL(SIAPROD.PRO_EXCL,'') &lt;&gt; 'S' AND
			NOT EXISTS (Select DISTINCT PART_NUMBER FROM CESTA_VALE (NOLOCK) where qtde&gt;0 and
			mes_ano&gt;=@DataInicio and
			mes_ano&lt;=@DataFimCesta AND PART_NUMBER = SIAPROD.PRO_CODI)
		GROUP BY pro_codi ) C
left join 
	DEPCATUMBI..SIAPROD S (NOLOCK) on S.PRO_CODI=C.PART_NUMBER and ISNULL(S.pro_excl,'') &lt;&gt;'S'
left join (select max(DESCRICAO) AS DESCRICAO,PART_NUMBER
			FROM CESTA_VALE (NOLOCK)
			GROUP BY PART_NUMBER ) CV on c.PART_NUMBER = cv.PART_NUMBER 
left join 
	CPAFORN F (NOLOCK) on S.PRO_FORN = F.FOR_CODI 
left join 
	SIAVEND FV (NOLOCK) on FV.VEN_CODI = F.FOR_VEND
left join
	PAISES P (NOLOCK) on F.FOR_PAIS_ID = P.PAIS_ID
--Menor preço de todos os contratos, independente do cliente - chamado 91631
left join (select 
				min(preco) as Preco, CT_PART_NUMBER 
			from
			(
			select 
				min(ct_preco) as PRECO, 
				CT_PART_NUMBER 
			from 
				VALE_CONTRATO_ITENS (NOLOCK)
			inner join VALE_CONTRATO vc (nolock) on
						vc.CT_ID = CT_ID_CONTRATO AND			
						ISNULL(VC.CT_SUSPENSO,'') &lt;&gt; 'S'				
			group by 
				ct_part_number
			union all
			select 
				min(ct_preco) as PRECO, 
				CT_PART_NUMBER 
			from 
				outros_contrato_itens (NOLOCK)
			inner join outros_contratos vc (nolock) on
						vc.CT_ID = CT_ID_CONTRATO AND			
						ISNULL(VC.CT_SUSPENSO,'') &lt;&gt; 'S'				
			group by 
				ct_part_number) Contratos
			Group by 
				Contratos.CT_PART_NUMBER) Menor_Preco on Menor_Preco.CT_PART_NUMBER = C.PART_NUMBER
--ITEM DE CONTRATO
LEFT JOIN (SELECT 
				'SIM' as CONTRATO, CI.CT_PART_NUMBER
			FROM  
				dbo.VALE_CONTRATO_ITENS CI (nolock)
			CROSS APPLY(SELECT * FROM 
				VALE_CONTRATO VC (NOLOCK) WHERE 
				VC.CT_ID = CI.CT_ID_CONTRATO AND
				ISNULL(VC.CT_SUSPENSO,'') &lt;&gt; 'S') VC			
			GROUP BY 
				CI.CT_PART_NUMBER) TC on TC.CT_PART_NUMBER = C.PART_NUMBER

