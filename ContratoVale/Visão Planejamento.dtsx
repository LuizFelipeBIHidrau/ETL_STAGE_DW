          SQLTask:Connection="{6B1C895D-F631-44DD-B99D-1B496EFE7C27}"
          SQLTask:SqlStatementSource="&#xA;MERGE&#xA;&#x9;GHT_DATAWAREHOUSE.DW.DimVisaoPlanejamento As Dest&#xA;Using &#xA;(&#xA;SELECT &#xA; Cast(V.DtInicio As Date) As DtInicio&#xA;,Cast(V.DtFim As Date) As DtFim&#xA;,V.CodProduto &#xA;,V.PartNumber &#xA;,V.Fornecedor &#xA;,V.Vendedor &#xA;,V.Origem &#xA;,V.Descricao &#xA;,IsNull(V.Contrato ,'Não') As Contrato&#xA;,V.CestaJan &#xA;,V.CestaFev &#xA;,V.CestaMar &#xA;,V.CestaAbr &#xA;,V.CestaMai &#xA;,V.CestaJun &#xA;,V.CestaJul &#xA;,V.CestaAgo &#xA;,V.CestaSet &#xA;,V.CestaOut &#xA;,V.CestaNov &#xA;,V.CestaDez &#xA;,V.AderJan &#xA;,V.AderFev &#xA;,V.AderMar &#xA;,V.AderAbr &#xA;,V.AderMai &#xA;,V.AderJun &#xA;,V.AderJul &#xA;,V.AderAgo &#xA;,V.AderSet &#xA;,V.AderOut &#xA;,V.AderNov &#xA;,V.AderDez &#xA;,V.GeralJan &#xA;,V.GeralFev &#xA;,V.GeralMar &#xA;,V.GeralAbr &#xA;,V.GeralMai &#xA;,V.GeralJun &#xA;,V.GeralJul &#xA;,V.GeralAgo &#xA;,V.GeralSet&#xA;,V.GeralOut &#xA;,V.GeralNov &#xA;,V.GeralDez &#xA;,ISNULL(V.GeralJan,0) + IsNull(V.GeralFev,0) + IsNull(V.GeralMar,0) + IsNull(V.GeralAbr,0) + IsNull(V.GeralJun,0) + IsNull(V.GeralJul,0) + IsNull(V.GeralAgo,0) + IsNull(V.GeralSet,0) + IsNull(V.GeralOut,0) + IsNull(V.GeralNov,0) + IsNull(V.GeralDez,0) As SomaVenda&#xA;,IIF(ISNULL(V.GeralJan,0) &gt; 0 ,1,0) + IIF(ISNULL(V.GeralFev,0) &gt; 0 ,1, 0) + IIF(ISNULL(V.GeralMar,0) &gt; 0 ,1, 0) + IIF(ISNULL(V.GeralAbr,0) &gt; 0 ,1, 0) + IIF(ISNULL(V.GeralMai,0) &gt; 0 ,1, 0) + IIF(ISNULL(V.GeralJun,0) &gt; 0 ,1, 0) + IIF(ISNULL(V.Geraljul,0) &gt; 0 ,1, 0) + IIF(ISNULL(V.GeralAgo,0) &gt; 0 ,1, 0)  + IIF(ISNULL(V.GeralSet,0) &gt; 0 ,1, 0) + IIF(ISNULL(V.GeralOut,0) &gt; 0 ,1, 0) + IIF(ISNULL(V.GeralNov,0) &gt; 0 ,1, 0) + IIF(ISNULL(V.GeralDez,0) &gt; 0 ,1, 0) As MesesVenda&#xA;,V.GeralCorrente &#xA;,V.Ponta &#xA;,V.Cor &#xA;,V.EstSP &#xA;,V.EstPA &#xA;,V.EstMG &#xA;,V.EstTR &#xA;,V.EstVIT &#xA;,V.EstCUR &#xA;,V.EstSL &#xA;,V.EstCER &#xA;,V.EstSER &#xA;,ISNull(V.EstSP ,0) + ISNULL(V.EstPA , 0)  + ISNULL(V.EstMG , 0)  + ISNULL(V