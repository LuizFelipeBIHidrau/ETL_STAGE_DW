                  description="Origem OLE DB"
                  name="STP_Item_de_Ponta"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="O tempo em segundos antes que um comando expire. Zero indica uma expiração infinita."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Especifica o nome do objeto de banco de dados usado para abrir um conjunto de linhas."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Especifica a variável que contém o nome do objeto de banco de dados usado para abrir um conjunto de linhas."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="O comando SQL a ser executado."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @DataInicio Date,
	    @DataFimCesta Date,	   
	    @DataFimVendas Date,
	    @PeriodoMov varchar(10)
Select @DataInicio = '2022-01-01'
Select @DataFimCesta = GETDATE()
Select @DataFimVendas = GETDATE()
 
--Pega a última data da cesta carregada no sistema
select	@DataFimCesta = max(mes_ano), 
		@DataFimVendas=EOMONTH(max(mes_ano)),
		@DataInicio =DATEADD(m, DATEDIFF(m, 0, max(mes_ano)-320), 0)  
from 
		cesta_vale (NOLOCK)
where 
		qtde&gt;0
--Chamado 
Select 
	 convert(varchar(10),@DataInicio,23) as DtInicio
	,convert(varchar(10),@DataFimCesta ,23)  as DtFim
	,s.pro_id
	,Cast(c.part_number As Varchar(16)) As PartNumber
	,Cast(S.PRO_FORN As varchar(16)) As Fornecedor
	,FV.VEN_NOME
	,P.Pais_Nome as Origem
	,Cast(IsNull(cv.descricao,c.PRO_DESC) As varchar(15)) as descricao	
	,TC.CONTRATO
	,max(case when month(C.mes_ano) = 1 then qtde else 0 end) CestaJan
	,max(case when month(C.mes_ano) = 2 then qtde else 0 end) CestaFev
	,max(case when month(C.mes_ano) = 3 then qtde else 0 end) CestaMar
	,max(case when month(C.mes_ano) = 4 then qtde else 0 end) CestaAbr
	,max(case when month(C.mes_ano) = 5 then qtde else 0 end) CestaMai
	,max(case when month(C.mes_ano) = 6 then qtde else 0 end) CestaJun
	,max(case when month(C.mes_ano) = 7 then qtde else 0 end) CestaJul
	,max(case when month(C.mes_ano) = 8 then qtde else 0 end) CestaAgo
	,max(case when month(C.mes_ano) = 9 then qtde else 0 end) CestaSet
	,max(case when month(C.mes_ano) = 10 then qtde else 0 end) CestaOut
	,max(case when month(C.mes_ano) = 11 then qtde else 0 end) CestaNov
	,max(case when month(C.mes_ano) = 12 then qtde else 0 end) CestaDez
	,ader.AderJan
    ,ader.AderFev
    ,ader.AderMar
    ,ader.AderAbr
    ,ader.AderMai
    ,ader.AderJun
    ,ader.AderJul
    ,ader.AderAgo
    ,ader.AderSet
    ,ader.AderOut
    ,ader.AderNov
    ,ader.AderDez
    ,Cast(geral.GeralJan as float) as GeralJan
    ,Cast(geral.GeralFev as float) as GeralFev
    ,Cast(geral.GeralMar as float) as GeralMar
