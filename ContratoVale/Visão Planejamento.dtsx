--Entregas
LEFT JOIN (Select 
				Produto,
				For_Codi,
				max(case when Entregas.RowNumber = 1 then Saldo else 0 end) OC_Qtde1,
				max(case when Entregas.RowNumber = 1 then Entrega else null end) OC_Data1,
				max(case when Entregas.RowNumber = 2 then Saldo else 0 end) OC_Qtde2,
				max(case when Entregas.RowNumber = 2 then Entrega else null end) OC_Data2,
				max(case when Entregas.RowNumber = 3 then Saldo else 0 end) OC_Qtde3,
				max(case when Entregas.RowNumber = 3 then Entrega else null end) OC_Data3,
				max(case when Entregas.RowNumber = 4 then Saldo else 0 end) OC_Qtde4,
				max(case when Entregas.RowNumber = 4 then Entrega else null end) OC_Data4,
				max(case when Entregas.RowNumber = 5 then Saldo else 0 end) OC_Qtde5,
				max(case when Entregas.RowNumber = 5 then Entrega else null end) OC_Data5,
				max(case when Entregas.RowNumber = 6 then Saldo else 0 end) OC_Qtde6,
				max(case when Entregas.RowNumber = 6 then Entrega else null end) OC_Data6
			From
			(select TOP 30000
						Produto,
						FOR_CODI,
						sum(Saldo) as SALDO,
						Entrega,
						ROW_NUMBER() OVER(Partition by Produto,FOR_CODI ORDER BY Produto,FOR_CODI) AS RowNumber
 
					from 
						OcPendentesFil    (NOLOCK)    
					where  
						isnull(Entrega,'')&lt;&gt;'' and
						Saldo&gt;0
					group by 
						Produto,
						FOR_CODI,
						Entrega
					order by 1,2,4) Entregas
			group by Entregas.Produto,Entregas.FOR_CODI) E on E.Produto = C.PART_NUMBER And E.FOR_CODI = S.PRO_FORN


