--ADERENCIA: QTDE VENDIDA PARA VALE
LEFT JOIN (select 
				M.ID_PROD,
				M.PRODUTO,
				max(case when Mes = 1 then QTDEVEND else 0 end) AderJan,
				max(case when Mes = 2 then QTDEVEND else 0 end) AderFev,
				max(case when Mes = 3 then QTDEVEND else 0 end) AderMar,
				max(case when Mes = 4 then QTDEVEND else 0 end) AderAbr,
				max(case when Mes = 5 then QTDEVEND else 0 end) AderMai,
				max(case when Mes = 6 then QTDEVEND else 0 end) AderJun,
				max(case when Mes = 7 then QTDEVEND else 0 end) AderJul,
				max(case when Mes = 8 then QTDEVEND else 0 end) AderAgo,
				max(case when Mes = 9 then QTDEVEND else 0 end) AderSet,
				max(case when Mes = 10 then QTDEVEND else 0 end) AderOut,
				max(case when Mes = 11 then QTDEVEND else 0 end) AderNov,
				max(case when Mes = 12 then QTDEVEND else 0 end) AderDez

			FROM 
				(select 
					M1.ID_PROD,M1.PRODUTO, SUM(QTDEVEND) AS QTDEVEND, MES 
				 from    
					MOVIMENTO..MARGEMGERENCIALGHT  M1(NOLOCK)
				 inner join 
					(select distinct 
							part_number 
					 from 
						cesta_vale (NOLOCK)) cv on cv.part_number = m1.produto
				WHERE 
					M1.CATEGORIA = 'FATURAMENTO' and
					M1.GERENCIA = 'VALE MINERAÇÃO'
					AND M1.DATA &gt;=@DataInicio  AND M1.DATA &lt;= @DataFimVendas
				GROUP BY
					M1.ID_PROD,
					M1.PRODUTO, 
					M1.MES
				) M
				GROUP BY 
					M.ID_PROD,
					M.PRODUTO) ADER on convert(varchar,rtrim(ltrim(ADER.ID_PROD))) = convert(varchar,rtrim(ltrim(S.pro_id))) and ADER.PRODUTO = S.PRO_CODI

----Vendas Gerais
LEFT JOIN (select 

				M.ID_PROD,
				M.PRODUTO,
				max(case when Mes = 1 then QTDEVEND else 0 end) GeralJan,
				max(case when Mes = 2 then QTDEVEND else 0 end) GeralFev,
				max(case when Mes = 3 then QTDEVEND else 0 end) GeralMar,
				max(case when Mes = 4 then QTDEVEND else 0 end) GeralAbr,
				max(case when Mes = 5 then QTDEVEND else 0 end) GeralMai,
				max(case when Mes = 6 then QTDEVEND else 0 end) GeralJun,
				max(case when Mes = 7 then QTDEVEND else 0 end) GeralJul,
				max(case when Mes = 8 then QTDEVEND else 0 end) GeralAgo,
				max(case when Mes = 9 then QTDEVEND else 0 end) GeralSet,
				max(case when Mes = 10 then QTDEVEND else 0 end) GeralOut,
				max(case when Mes = 11 then QTDEVEND else 0 end) GeralNov,
				max(case when Mes = 12 then QTDEVEND else 0 end) GeralDez


			FROM 
				(select 
					M1.fornecedor,M1.PRODUTO, SUM(QTDEVEND) AS QTDEVEND, Month(DtMovto) as Mes,
					(SELECT TOP 1 PRO_ID FROM SIAPROD (NOLOCK) WHERE PRO_CODI=M1.PRODUTO AND PRO_FORN=M1.fornecedor) AS ID_PROD
				 from    
					(
						select  
							produto,
							fornecedor,
							CONVERT(DATE, '01/'+mesano, 103) as DtMovto,
							sum(quantidade) as QTDEVEND 
						from 
							movimento..produtovendas (NOLOCK)
						where 
							MesAno not like'%ZZ%' 
						group by 
							produto,
							fornecedor,
							mesano ) M1
				 WHERE 													
					 M1.DtMovto &gt;=@DataInicio  AND M1.DtMovto &lt;=@DataFimVendas
				GROUP BY
					M1.fornecedor,M1.PRODUTO,M1.DTMOVTO
						) M GROUP BY M.ID_PROD,M.PRODUTO ) GERAL on GERAL.ID_PROD = S.pro_id and GERAL.PRODUTO = S.PRO_CODI


----Vendas Gerais Mes Corrente
LEFT JOIN (select 

				M.ID_PROD,
				M.PRODUTO,
				QTDEVEND As GeralCorrente

			FROM 
				(select 
					M1.fornecedor,M1.PRODUTO, SUM(QTDEVEND) AS QTDEVEND, 
					(SELECT TOP 1 PRO_ID FROM SIAPROD (NOLOCK) WHERE PRO_CODI=M1.PRODUTO AND PRO_FORN=M1.fornecedor) AS ID_PROD
				 from    
					(
						select  
							produto,
							fornecedor,
							CONVERT(DATE, '01/'+mesano, 103) as DtMovto,
							sum(quantidade) as QTDEVEND 
						from 
							movimento..produtovendas (NOLOCK)
						where 
							MesAno not like'%ZZ%' 
						group by 
							produto,
							fornecedor,
							mesano ) M1
				 WHERE 													
					 M1.DtMovto &gt;=DATEADD(month, DATEDIFF(month, 0, getdate()), 0)  AND M1.DtMovto &lt;=EOMONTH(getdate())
				GROUP BY
					M1.fornecedor,M1.PRODUTO,M1.DTMOVTO
						) M GROUP BY M.ID_PROD,M.PRODUTO,M.QTDEVEND ) GERALCorrente on GERALCorrente.ID_PROD = S.pro_id and GERALCorrente.PRODUTO = S.PRO_CODI

