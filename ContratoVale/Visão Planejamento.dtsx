--CORES
Left Join (select SPCOR.pro_id,SPCOR.pro_codi,CASE
		WHEN ISNULL(SPCOR.PRO_SREP,'') = 'S' THEN 'Preto'
		WHEN ISNULL(SPCOR.PRO_EXCESSOESTOQUE,0) = 2 THEN 'Amarelo'
		WHEN ISNULL(SPCOR.PRO_EXCESSOESTOQUE,0) = 1 THEN 'Azul'
		WHEN ISNULL(SPCOR.PRO_ITEMROXO,'N') = 'S' THEN 'Roxo'
		WHEN (ISNULL(SPCOR.PRO_SREP,'') &lt;&gt; 'S' AND ISNULL(SPCOR.PRO_EXCESSOESTOQUE,0) NOT IN ('2','1') AND ISNULL(SPCOR.PRO_REPO,'') &lt;&gt; 'S') THEN 'Verde'
		  ELSE ''
		End As COR from SIAPROD SPCOR (NOLOCK) where ISNULL(SPCOR.PRO_EXCL,'') &lt;&gt; 'S') COR ON COR.pro_id = S.PRO_ID AND COR.PRO_CODI = S.PRO_CODI

