			SF.PFA_PROD,
			SF.PFA_FORN,
			SF.Pfa_loteEconomico as LoteEcn
		from 
			SIAPFAB SF (NOLOCK)
		INNER JOIN SIAPROD S (NOLOCK) ON
			S.PRO_CODI = SF.PFA_PROD and
			S.PRO_FORN = SF.PFA_FORN AND
			ISNULL(S.PRO_EXCL,'')&lt;&gt;''
		where 
			isnull(pfa_loteeconomico,'')&lt;&gt;'')LE on LE.PFA_PROD=C.PART_NUMBER AND LE.PFA_FORN = S.PRO_FORN

--Gerencia que mais vendeu
LEFT OUTER JOIN ( SELECT * from (SELECT 
									PRODUTO,
									REALIZADO,
									GERENCIA, 
									ROW_NUMBER() OVER (PARTITION BY PRODUTO ORDER BY REALIZADO DESC) as posicao
								FROM
										(				SELECT 
															M.PRODUTO, 
															SUM(M.FATCOMST - M.DEVOLCOMST) AS REALIZADO, 
															M.GERENCIA AS GERENCIA
							
														FROM 
															MOVIMENTO..MARGEMGERENCIALGHT M WITH(NOLOCK)
														where 
															M.DATA&gt;=@DataInicio and M.DATA &lt;=@DataFimVendas 							
														GROUP BY 
															M.PRODUTO, M.GERENCIA) DADOS) resultado
								where 
									resultado.posicao = 1

				)  GER on GER.PRODUTO = C.PART_NUMBER
