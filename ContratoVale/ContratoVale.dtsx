                    <property
                      dataType="System.String"
                      description="O comando SQL a ser executado."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT V.Tipo As Tipo,
      E.CodEmpresa As IdFilial,	
      V.Filial As Filial,
      V.CnpjFilial As CnpjFilial,
      V.Cliente As Cliente,
      V.Gerencia As Gerencia,
      V.Regional As Regional,
      V.Data As Data,
     Convert(Char(6),V.Data, 112) As DataPeriodo,
      Coalesce(D.PDP_PNOV,V.Produto) As Produto, 
      V.Fornecedor As Fornecedor,
      V.Descrição As Descrição,
      V.Linha As Linha,
      V.Familia As Familia,
      V.NumPedidoVale As NumPedidoVale,
      V.NumContrato As NumContrato,
      IIf(V.DescContratoAtual Is Null,'Spot',V.DescContratoAtual) As DescContratoAtual,
      V.Pedido As Pedido,
      V.UF As UF,
      V.Vendedor As Vendedor,
      V.Romaneio As Romaneio,
      V.Danfe As Danfe,
      Coalesce(I1.Tipo,I3.Tipo) As Homologacao,
      V.Qtde As Qtde,
	  P.pro_dinc As DtCadastro,
      V.FatComImp As FatComImp,
	  H.Planta,
	  H.Estado,
	  H.UF As HomologaUF
  FROM Movimento..AcompanhamentoContratosVale As V (NOLOCK) 
    Left join DEPCATUMBI..Siapdpa As D (NoLock) On (V.Produto = D.PDP_PANT)
    Left Join DEPCATUMBI..ItensHomologados As I1 (NoLock) On(Coalesce(D.PDP_PNOV,V.Produto) = I1.Produto And V.Fornecedor = I1.Fornecedor And I1.Tipo = 'H')
    Left Join (Select I2.Produto,(Select 'H') As Tipo From DEPCATUMBI..ItensHomologados As I2 (Nolock) Where I2.Tipo is null) As I3 On (V.Produto = I3.Produto)
    Left Join DEPCATUMBI..Siaprod As P (nolock) On (Coalesce(D.PDP_PNOV,V.Produto) = P.PRO_CODI And V.Fornecedor = P.PRO_FORN)
    Left Join Comercial_St.Stage.HomologaEstados As H On (V.Produto = H.ProdNumPeca COLLATE Latin1_General_CI_AS And V.Fornecedor = H.NomeFornecedor COLLATE Latin1_General_CI_AS)
    Left Join COMERCIAL_ST.Stage.CadEmpresa As E On (V.Filial = E.DeparaVale COLLATE Latin1_General_CI_AS )</property>
