	  ,E.Ght_Est
	  ,E.Ght_Fatura
	  ,M1.CmvDre As CustoVenda
	  ,E.Ght_CustoEst
	  ,E.Ght_CustoEst / E.Ght_CustoVenda As Giro
	  ,M1.FatuMkp As FaturaMarkup
	  ,M1.MkpNew As NovMarkup
	  ,PN.Qtde As EstPN
	  ,COUNT(V.Produto) Over(Partition by V.Filial, V.Produto, V.PeriodoMov Order by V.PeriodoMov Rows Between Unbounded Preceding And Current Row) As Registro
	  ,M.Media As MediaVendaAno
	  ,O.EstadoPonta
	  ,E.Ult_12_MesesSemVendas
	  ,P.ProdStatusCor As StatusCor
	  ,IIF(PN.Qtde &lt; M.Media, 'Em Ruptura', 'OK') As EstRuptura
  FROM ST.ContratoVale As V (NOLOCK)
  Left Join ( 
  select E.CodEmpresa, P.ProdNumPeca, P.NomeFornecedor,T.DataPeriodo, F.Ght_Est, F.Ght_Fatura, F.Ght_CustoVenda, F.Ght_CustoEst, F.Uf, F.Ult_12_MesesSemVendas from GHT_DATAWAREHOUSE.DW.FatoEstoque As F
  inner join GHT_DATAWAREHOUSE.DW.DimCadProduto As P ON (F.IDSKProduto = P.IDSKProduto)
  inner Join GHT_DATAWAREHOUSE.DW.DimTempoMes As T ON (F.IDSKTempoMes = T.IDSKTTempMes) 
  Inner Join GHT_DATAWAREHOUSE.DW.DimCadEmpresa As E ON (F.IDSKEmpresa = E.IDSKEmpresa))
   As E ON (V.IdFilial = E.CodEmpresa And V.Produto = E.ProdNumPeca And V.Fornecedor = E.NomeFornecedor And V.PeriodoMov = E.DataPeriodo And V.UF = E.Uf)
  Left Join (
Select Filial, Produto, Sum(Qtde) / COUNT(Distinct( PeriodoMov)) As Media, LEFT(PeriodoMov, 4) As Ano From ST.ContratoVale
Where Tipo = 'faturado' Group by Filial, Produto, LEFT(PeriodoMov, 4)) As M On (V.Filial = M.Filial And V.Produto = M.Produto And LEFT(V.PeriodoMov, 4) = M.Ano)	
  Left Join (Select CodEmpresa, ProdNumPeca, PeriodoMov, Sum(QuantEstoque) Qtde, UF from ST.EstoqueFilial Group by CodEmpresa, ProdNumPeca,PeriodoMov, UF ) As PN On (V.IdFilial = PN.CodEmpresa And V.Produto = PN.ProdNumPeca And V.PeriodoMov = PN.PeriodoMov And V.UF = PN.UF)
  Left join (select Distinct PartNumber, EstadoPonta from GHT_DATAWAREHOUSE.DW.DimCadContrato Where RegistroVersao = 'Atual' And  TipoContrato = 'Contrato Vale' ) As O ON (V.Produto = O.PartNumber)
  Left Join ST.CadProduto As P On (V.Produto = P.ProdNumPeca And V.Fornecedor = P.NomeFornecedor And V.PeriodoMov = P.PeriodoMov)
  Left Join (Select Distinct CodFilial, Produto, Fornecedor, Periodo, CmvDre, FatuMkp,MkpNew from GHT_DATAWAREHOUSE.DW.DimMargemGerencial 
  )as M1 ON (V.Produto = M1.Produto And V.Fornecedor = M1.Fornecedor And V.PeriodoMov = M1.Periodo and V.IdFilial = M1.CodFilial)</property>
