                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT V.Tipo
      ,V.IdFilial
      ,V.Filial
      ,V.CnpjFilial
      ,V.Cliente
      ,V.Gerencia
      ,V.Regional
      ,V.Data
