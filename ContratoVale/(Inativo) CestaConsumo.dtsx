                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Select 
	   (Select 'Faturado') As Tipo
      ,(Select 'Cesta de Consumo') As TpLanc
	  ,C.CliApelido As Cliente
	  ,cast(G.GerNome As Varchar(20)) As Gerencia
	  ,Cast(R.RegNome As Varchar(30)) As Regional
	  ,F.DataMov As Data
	  ,F.Periodo As PeriodoMov
	  ,Cast(F.PartNumber As varchar(16)) As Produto
	  ,Cast(F.Descricao As varchar(200)) As Descricao
	  ,L.DescLin As Linha
	  ,T.NumContrato
	  ,T.DescricaoCapa
	  ,C.CliEstado As UF
	  ,F.Qtde As Qtde
	  ,F.Planta
                     ,OEMspot

from Stage.CestaConsumo As F (NOLOCK) 
Left join Stage.CadPlanta As P (NOLOCK) ON (F.Planta = P.CodPlanta)
Left Join Stage.CadCliente As C (NOLOCK) ON (P.CodCliente =C.CliCod)
Left Join Stage.CadGerencia As G (NOLOCK) ON (C.CliGerencia = G.CodGer)
Left Join Stage.CadRegionais As R (NOLOCK) ON (C.CliRegional = R.CodReg)
Left Join Stage.CadContrato As T (NOLOCK) ON  (F.PartNumber = T.PartyNumber And F.Periodo &gt;= T.Periodo)
Left join Stage.CadLinha As L (NOLOCK) ON (T.Linha = L.CodLin)</property>
