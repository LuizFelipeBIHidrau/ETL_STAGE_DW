                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With Cte As 
 (
 Select 
		D.DescrMeta As MetaDescr,
		D.Colaboradores As Representante,
		D.Mina,
		D.Periodo,
		D.IdFaixaMeta As Faixa		
	From DW.DimMovMetaVale As D
	Inner Join GHT_STAGE.ST.DepartamentoMeta As M on (D.DescrMeta = M.MetaDescr And D.Colaboradores = M.Representante And D.Mina = M.Mina And D.Periodo = M.Periodo And M.FaturamentoGht &gt;= D.FaturamentoMeta)	
)
SELECT Distinct
       M.MetaDescr
      ,M.SubMetaDescr
      ,M.Categoria
      ,M.Representante
      ,M.Mina
      ,M.Periodo
      ,M.PeriodoMov
      ,M.FaturamentoGht
	  ,V1.FaturamentoMeta
	  ,IsNull(V.Fx,0) As FaixaMeta
	 -- ,IIF(And(M.MetaDescr = V.MetaDescr , M.Representante = V.Representante And M.Mina = V.Mina And M.Periodo = V.Periodo , 'Faixa 0', 'Sem Meta') As Verifica
  From GHT_STAGE.ST.DepartamentoMeta As M (NOLOCK)
  left Join (
	Select MetaDescr, Representante, Mina, Periodo,  Max(Faixa) As Fx From Cte 	
  Group by MetaDescr, Representante, Mina, Periodo
  )
 As V On (M.MetaDescr = V.MetaDescr And M.Representante = V.Representante And M.Mina = V.Mina And M.Periodo = V.Periodo)  
  Left Join DW.DimMovMetaVale As V1 On (M.MetaDescr = V1.DescrMeta And M.Representante = V1.Colaboradores And M.Mina = V1.Mina And M.Periodo = V1.Periodo And V.Fx = V1.IdFaixaMeta) 
  Inner Join DW.DimMovMetaVale As V2 On (M.MetaDescr = V2.DescrMeta And M.Representante = V2.Colaboradores And M.Mina = V2.Mina And M.Periodo = V2.Periodo)</property>
