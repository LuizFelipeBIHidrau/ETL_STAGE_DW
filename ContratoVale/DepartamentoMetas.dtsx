                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
       M.MetaDescr
      ,M.SubMetaDescr
      ,'Global' As Categoria 
      ,M.Representante
      ,M.Mina
      ,M.Periodo
       ,Concat(Right(M.Periodo,2),'/',Left(M.Periodo,4)) As PeriodoMov
      ,Sum(Faturamento) As Faturamento
  FROM DepartamentoMeta As M (NOLOCK)
  Where M.MetaDescr = 'META GLOBAL'
  Group by M.MetaDescr, M.SubMetaDescr, M.Representante, M.Mina, M.Periodo, Concat(Right(M.Periodo,2),'/',Left(M.Periodo,4))
Union 
SELECT 
       M.MetaDescr
      ,M.SubMetaDescr
      ,'Representante' As Categoria 
      ,M.Representante
      ,M.Mina
      ,M.Periodo
       ,Concat(Right(M.Periodo,2),'/',Left(M.Periodo,4)) As PeriodoMov
      ,Sum(Faturamento) As Faturamento
  FROM DepartamentoMeta As M (NOLOCK)
  inner Join DescrMetaVendedor As V (NOLOCK) ON (M.Representante = V.Representante) 
