                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With Cubo As (
SELECT
GERENCIA AS GERENCIA,
REGIONAL AS REGIONAL,
FILIAL AS FILIAL,
PERIODO AS PERIODO,
ANO AS ANO,
MES AS MES,
DIA AS DIA,
DATA AS DATA,
PRODUTO AS PRODUTO,
A.DESCRICAO AS [DESCRIÇÃO],
FORNECEDOR AS FORNECEDOR,
LTRIM(RTRIM(LINHA)) AS LINHA,
LTRIM(RTRIM(FAMILIA)) AS [FAMÍLIA],
LTRIM(RTRIM(MARCA)) AS MARCA,
APELIDO AS CLIENTE,                --Adicionar Mina
UF AS UF,
MUNICIPIO AS CIDADE,
V.VEN_NOME AS  VENDEDOR_ATUAL,
V2.VEN_NOME AS VENDENDOR_SUBSTITUTO,
B.DESCRICAO AS SEGMENTO_MERCADO,
CAST(QTDEVEND AS VARCHAR) AS QTDE,
SUM(QTDEVEND) AS QTDE2,
SUM(FATCOMST)-SUM(DEVOLCOMST) AS FATURAMENTO,
H.REP_RAZA AS REPRESENTANTE_INTERNO,  -- Adicionar Representante
A.REP_EXTERNO AS REPRESENTANTE_EXTERNO,
C.CLI_CGC AS CNPJ,
GRUPO_INTRA AS GRUPO,
COR,
NUM_PEDIDOCLIENTE AS OC_VALE
FROM MOVIMENTO..MARGEMGERENCIALGHT A (NOLOCK)
LEFT JOIN DEPCATUMBI..CPACLIE C (NOLOCK) ON (A.COD_CLIENTE = C.CLI_CODI)
LEFT JOIN DEPCATUMBI..SIAREPR H (NOLOCK) ON (C.CLI_REPR = H.REP_CODI)
LEFT JOIN DEPCATUMBI..SEGMENTOMERCADO B (NOLOCK) ON (C.CLI_SEGMENTO = B.SEGMENTO_ID)

LEFT JOIN (SELECT VEN_CODI, VEN_NOME
				FROM MOVIMENTO..SIAVEND (NOLOCK)
					WHERE EMPRESA = 'HIDRAU'
			) V ON (V.VEN_CODI = C.CLI_VEND)

LEFT JOIN (SELECT VEN_CODI, VEN_NOME
				FROM MOVIMENTO..SIAVEND (NOLOCK)
					WHERE EMPRESA = 'HIDRAU'
			) V2 ON (V2.VEN_CODI = C.CLI_VEND_SUB)

WHERE ANO &gt;= '2022' And V.VEN_NOME IN('GUILHERME DORIA SANTOS','LUCAS ALEXANDRE','LUCIANO GLAYDSON LIMA','MATHEUS PEREIRA ROSA')
GROUP BY ANO, MES, PERIODO, PRODUTO, A.DESCRICAO, FORNECEDOR, LTRIM(RTRIM(LINHA)), FAMILIA, MARCA,GERENCIA, H.REP_RAZA, V.VEN_NOME, V2.VEN_NOME,
APELIDO, UF, VENDEDOR_CADASTRO, FILIAL, MUNICIPIO, B.DESCRICAO, DIA, QTDEVEND, DATA, REGIONAL, C.CLI_CGC, GRUPO_INTRA, COR, NUM_PEDIDOCLIENTE,
A.REP_EXTERNO)

--Meta1
Select  
	1 As CodMeta,
	'META GLOBAL' As Meta,
    '2022 Meta Global' As DescricaoMeta,
	'GeralGht' As Vendedor, 
	'GeralGht' As REPRESENTANTE_INTERNO,
	'GeralGht' As CLIENTE,
	PERIODO,
	Sum(FatComSt) As Faturamento

From MOVIMENTO..MARGEMGERENCIALGHT
Where PERIODO &gt;= '202209' And CATEGORIA = 'Faturamento'
Group by PERIODO
UNION

--Meta2
Select 
	2 As CodMeta,
	'PEÇA + ILUMINAÇÃO SISTEMAS  (SEM EGP, SISTEMAS, ACESSÓRIOS DE OFICINA E BIZA)' As Meta,
    '2022 Vale mineração MG/PA - Peças' As DescricaoMeta,
	M.Vendedor_Atual As Vendedor, 
	M.REPRESENTANTE_INTERNO,
	M.CLIENTE,
	M.PERIODO,
	Sum(M.FATURAMENTO) As Faturamento
From Cubo As M
Where M.GERENCIA = 'VALE MINERAÇÃO' And M.GRUPO IN ('CAMINHÕES LEVES','',  'SERVIÇOS') And M.FAMÍLIA Not IN('ACESSORIOS OFICINA', 'INOVACAO', 'IMPULSOR', 'INOVACAO BIZA')
And M.MARCA Not IN ('BIZA', 'BIZA PREMIUM', 'BIZA START','BIZA STD')
Group by M.PERIODO, M.VENDEDOR_ATUAL,  M.CLIENTE, M.REPRESENTANTE_INTERNO

UNION
Select 
	2 As CodMeta,
	'PEÇA + ILUMINAÇÃO SISTEMAS  (SEM EGP, SISTEMAS, ACESSÓRIOS DE OFICINA E BIZA)' As Meta,
    '2022 Vale mineração MG/PA - Peças' As DescricaoMeta,
	M.Vendedor_Atual As Vendedor, 
	M.REPRESENTANTE_INTERNO,
	M.CLIENTE,
	M.PERIODO,
	Sum(M.FATURAMENTO) As Faturamento
From Cubo As M
Where M.GERENCIA = 'VALE MINERAÇÃO' And M.GRUPO IN ('CAMINHÕES LEVES','',  'SERVIÇOS','SISTEMAS') And M.FAMÍLIA = 'ILUMINACAO'
Group by M.PERIODO, M.VENDEDOR_ATUAL,  M.CLIENTE, M.REPRESENTANTE_INTERNO
UNION
--Meta3
Select 
	3 As CodMeta,
	'BIZA + INOVAÇÃO BIZA (SEM ACESSORIOS DE OFICINA, INOVAÇÃO, EGP E SISTEMAS)' As Meta,
    '2022 Vale mineração MG/PA - BIZA' As DescricaoMeta,
	M.Vendedor_Atual As Vendedor, 
	M.REPRESENTANTE_INTERNO,
	M.CLIENTE,
	M.PERIODO,
	Sum(M.FATURAMENTO) As Faturamento
From Cubo As M
Where M.GERENCIA = 'VALE MINERAÇÃO' And M.GRUPO IN ('CAMINHÕES LEVES','',  'SERVIÇOS') And M.FAMÍLIA Not IN('ACESSORIOS OFICINA', 'INOVACAO', 'IMPULSOR', 'INOVACAO BIZA')
And M.MARCA IN ('BIZA', 'BIZA PREMIUM', 'BIZA START','BIZA STD')
Group by M.PERIODO, M.VENDEDOR_ATUAL,  M.CLIENTE, M.REPRESENTANTE_INTERNO
UNION
--Meta4
Select 
	4 As CodMeta,
	'ACESSÓRIOS DE OFICINA + INOVACAO (SEM BIZA, EGP E SISTEMAS)' As Meta,
    '2022 - Vale min - novos negocios' As DescricaoMeta,
	M.Vendedor_Atual As Vendedor, 
	M.REPRESENTANTE_INTERNO,
	M.CLIENTE,
	M.PERIODO,
	Sum(M.FATURAMENTO) As Faturamento
From Cubo As M
Where M.GERENCIA = 'VALE MINERAÇÃO' And M.FAMÍLIA IN('ACESSORIOS OFICINA', 'INOVACAO', 'IMPULSOR', 'INOVACAO BIZA')
Group by M.PERIODO, M.VENDEDOR_ATUAL,  M.CLIENTE, M.REPRESENTANTE_INTERNO</property>
