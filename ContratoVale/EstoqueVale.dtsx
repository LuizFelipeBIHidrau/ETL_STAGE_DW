                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Select 
	Convert(Varchar(16),P.ProdNumPeca,0) As PartNumber,
	Convert(Varchar(16),P.NomeFornecedor, 0) As Fornecedor,
	T.DataPeriodo AS Periodo,
	Sum(F.QtdeVendVale) As QtdeVendida,
	Sum(F.QuantEstoque) As QtdeEstoque,	
	Sum(F.FaturamentoVale) As Faturamento,
	Sum(F.CustoVenda) As CustoVenda,
	Sum(F.CustoEstoque) As CustoEstoque,
	F.UfEstoque As EstadoPonta 
from GHT_DATAWAREHOUSE.DW.FatoEstoque As F
INNER Join Ght_DataWarehouse.Dw.DimCadProduto As P (Nolock) ON (F.IDSKProduto = P.IDSKProduto )
INNER Join Ght_DataWarehouse.Dw.DimTempoMes As T (NoLock) ON (F.IDSKTempo = T.IDSKTTempMes)
Left Join GHT_DATAWAREHOUSE.DW.DimMargemGerencial As M (Nolock) ON (F.IDSKMargem = M.IdskMargem)
Left Join (Select Distinct PartyNumber, Periodo, EstadoPonta from GHT_STAGE.ST.CadContrato Where TipoContrato = 'Contrato Vale') As V ON(P.ProdNumPeca = V.PartyNumber 
And F.UfEstoque = V.EstadoPonta And T.DataPeriodo &gt;= V.Periodo)
Group By P.ProdNumPeca, P.NomeFornecedor, T.DataPeriodo, F.UfEstoque</property>
