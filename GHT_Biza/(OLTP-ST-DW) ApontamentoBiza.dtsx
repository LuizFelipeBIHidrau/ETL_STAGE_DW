                    <property
                      dataType="System.String"
                      description="O comando SQL a ser executado."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Select  
 A.Numero, 
 B.Sequencia,
 B.Ordem As NumeroOf,
 C.PRO_CODI As PartNumber,
 C.PRO_DESC As Descricao,
 C.PRO_FORN As Fornecedor,
 C.PRO_CUSTOSEMIMPOSTOS As CustoProduto,
 Convert(Char(6), A.Data, 112) As Periodo,
 A.Data As DataApontamento,
 Case When A.Tipo = 'P' Then  ConVert(varchar,DATEADD(Second, DATEDIFF(SECOND, LAG(A.Data) Over (Partition by A.Sequencia, A.Operador Order by A.Data), A.Data) + 86400000,0),114 ) When 
 A.Tipo = ' ' Then '' When 
 A.Tipo = 'I' Then 'Início' When
 A.Tipo = 'R' Then 'Reinício' When 
 A.Tipo = 'P' And TempoRealizado Is null And A.Data Is Null Then 'Erro' When 
 A.Tipo = 'T' Then ConVert(varchar,DATEADD(Second, DATEDIFF(SECOND, LAG(A.Data) Over (Partition by A.Sequencia, A.Operador Order by A.Data), A.Data) + 86400000,0),114 ) 
 Else '' End As TempoLote,
 B.Quantidade As QtdeProduzida,
 A.quantidade As QtdeApontada,
 A.QuantidadePerda As QtdePerda,
 B.Operacao,
 B.Maquina,
 A.MaquinaPerda  As MaquinaPerda,
 B.EmProducaoOperacoesId, 
 B.Situacao,
 E.Descricao As MotivoParada,
 F.Descricao As MotivoRefugo,
 D.Nome As Operador,
 D.Entrada As HrEntradaOperador,
 D.Saida As HrSaidaOperador,
 B.TempoPrevisto,
 B.TempoRealizado,
 Case When E.Movimento = 'I' Then 'INTERNO' When E.Movimento = 'E' Then 'EXTERNO' Else 'TODOS' End As MovParada,
 Case When F.Movimento = 'I' Then 'INTERNO' When F.Movimento = 'E' Then 'EXTERNO' Else 'TODOS' End As MovRefugo,
 Case When A.Tipo = 'I' Then 'Início' When A.Tipo = 'P' Then 'Parada' When A.Tipo = 'R' Then 'Reinício' When A.Tipo = 'T' Then 'Termino' Else '' End As Tipo,
 A.Observacao,
 IIF(A1.Seque Is Null, 'N', 'S') As ProdutoFinal

from FabrilPr..ApontamentoHoras As A (NOLOCK)
Left Join FabrilPr..EmProducaoOperacoes As B (NOLOCK) ON (A.Sequencia = B.EmProducaoOperacoesId) 
Left Join FabrilPr..SiaProd C (Nolock) On (B.Produto = C.Pro_Id)
Left Join FabrilPr..OperadorFabril As D (Nolock) On (A.Operador = D.Operador)
Left Join FabrilPr..TipoParada As E (NOLOCK) ON (A.MotivoParada = E.Codigo)
Left Join FabrilPr..TipoParada As F (NOLOCK) ON (A.MotivoRefugo = F.Codigo)
Left Join (
Select Max(A.Numero) Seque, B.Ordem from FabrilPr..ApontamentoHoras As A (NOLOCK) 
Inner Join FabrilPr..EmProducaoOperacoes As B (NOLOCK) ON (A.Sequencia = B.EmProducaoOperacoesId) 
Group by B.Ordem ) As A1 On (A.Numero = A1.Seque And B.Ordem = A1.Ordem)
Where A.data &gt;= '2022.01.01'</property>
