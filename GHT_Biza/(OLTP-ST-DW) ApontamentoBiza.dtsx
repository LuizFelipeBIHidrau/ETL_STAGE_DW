	  ,(DATEDIFF(second, Lag(A.DataApontamento) Over( partition by A.Sequencia, A.NumeroOf, A.Maquina, A.Operador Order by A.DataApontamento), A.DataApontamento) + IIF(DATEDIFF(second, Cast(A.HrEntradaOperador As time),Cast(A.DataApontamento As time)) &lt; 0,DATEDIFF(second, Cast(A.HrEntradaOperador As time),Cast(A.DataApontamento As time)), 0 ))
  + IIF(DATEDIFF(second, Cast(A.DataApontamento As time),Cast(A.HrSaidaOperador As time))&lt; 0 ,DATEDIFF(second, Cast(DataApontamento As time),Cast(HrSaidaOperador As time)), 0) As SaldoHrSegundo
	  ,A.CustoProduto As CustoProdutoGht	  
	  ,B2.Custo_STD As CustoBiza
	  ,B2.QtdTotalProduto As QtdTotalBiza
	  ,B2.TotalProduto As TotalBiza
	  ,B.IdSetor
	  ,B.Setor
	  ,B.Lider
	  ,A.Observacao
	  ,IIF(B2.Ordem is not null, 'Faturou', 'Em Produção') As ProdutoStatus
      ,MovParada
      ,MovRefugo
	  ,ProdutoFinal
  FROM ST.BizaApontamento As A (NOLOCK)
  Left Join GHT_OLTP.OLTP.CadsetorBiza As B On (A.Operador = B.Operador And A.Maquina = B.Maquina)  
  Left Join (Select Distinct Ordem, PartNumber, Custo_STD, QtdTotalProduto, TotalProduto  From ST.BizaEntradaProducao) As B2 On (A.NumeroOf = B2.Ordem And A.PartNumber = B2.PartNumber)
